AGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,kBAAA;AACA,OAXA;AAaA,KAlHA;AAmHA,IAAA,UAnHA,wBAmHA;AAAA;;AAAA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,wCAAA,EAAA,EAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;;AACA,YAAA,MAAA,CAAA,QAAA,IAAA,MAAA,CAAA,SAAA,CAAA,QAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA;AACA,OAbA;AAeA;AAnIA,GAzBA;AA+JA,EAAA,OA/JA,qBA+JA;AAAA;;AACA,SAAA,qBAAA;AACA,SAAA,WAAA;AACA,IAAA,UAAA,CAAA;AAAA,aAAA,MAAA,CAAA,UAAA,EAAA;AAAA,KAAA,EAAA,GAAA,CAAA;AAEA;AApKA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <Header/>\r\n        <div class=\"background_main\">\r\n            <div class=\"center\">\r\n                <div style=\"height: 15px\"></div>\r\n                    <div style=\"display: flex;\">\r\n                        <div class=\"home_style\">\r\n                            <div v-if=\"edit==false\" class=\"border_st\">\r\n                                <div class=\"space-edit\">\r\n                                    <div style=\"display: flex\">\r\n                                         <a class=\"sub_link\" v-on:click=\"back_home()\"><img class=\"img_p\" :src=\"url+'/media/profile/'+data_mess.username+'/'+data_mess.username+'.png'\"></a>\r\n                                        <div class=\"username_p\">\r\n                                            <a class=\"sub_link\" v-on:click=\"back_home()\">{{data_mess.username}}</a>\r\n                                        </div>\r\n\r\n\r\n                                    </div>\r\n                                    <div v-if=\"access==true\" style=\"display: flex\">\r\n                                        <b-dropdown\r\n                                             variant=\"link\"\r\n                                             toggle-class=\"text-decoration-none\"\r\n                                             no-caret>\r\n                                        <template #button-content>\r\n                                          <p><span class=\"select\">&hellip;</span></p>\r\n                                        </template>\r\n                                        <b-dropdown-item v-on:click=\"edit_publ()\"><p><span class=\"a_style\">Редактировать</span></p></b-dropdown-item>\r\n                                        <b-dropdown-item v-on:click=\"delete_publ()\"><p><span class=\"a_style_red\">Удалить</span></p></b-dropdown-item>\r\n                                      </b-dropdown>\r\n                                    </div>\r\n                                </div>\r\n                                <div style=\"height: 15px\"></div>\r\n                                <div class=\"just-line-break\">\r\n                                    {{data_mess.message}}\r\n                                </div>\r\n                                <div style=\"height: 15px\"></div>\r\n                                <div style=\"display: flex\">\r\n                                    {{data_mess.datetime}}\r\n                                    <div class=\"edir_purl_style\" v-if=\"data_mess.edit==true\">\r\n                                        <span class=\"edit_style\">изменено</span>\r\n                                    </div>\r\n                                </div>\r\n                                <hr>\r\n                                <div style=\"display: flex\">\r\n                                    <img class=\"img_input_m\" :src=\"url+'/media/profile/'+username+'/'+username+'.png'\">\r\n                                        <textarea-autosize\r\n                                          placeholder=\"Добавьте комментарий...\"\r\n                                          class=\"input_mes\"\r\n                                          v-model=\"comment\"\r\n                                          :min-height=\"30\"\r\n                                          :max-height=\"180\"\r\n                                         />\r\n                                    <button class=\"button_style but_input_m\" v-on:click=\"save_comment()\">Отправить</button>\r\n                                </div>\r\n                                <hr>\r\n                                <div v-for=\"mess in all_comment\">\r\n                                    <div class=\"space-edit\">\r\n                                        <div style=\"display: flex\">\r\n                                            <a class=\"sub_link\" v-on:click=\"go_page(mess.username)\"><img class=\"img_p\" :src=\"url+'/media/profile/'+mess.username+'/'+mess.username+'.png'\"></a>\r\n                                            <div class=\"username_p\">\r\n                                                <a class=\"sub_link\" v-on:click=\"go_page(mess.username)\">{{mess.username}}</a> · {{mess.datetime}}\r\n                                            </div>\r\n                                        </div>\r\n                                        <div class=\"username_p\" v-if=\"mess.edit==true\">\r\n                                            <span class=\"edit_style\">изм</span>\r\n                                        </div>\r\n                                    </div>\r\n                                    <div style=\"margin-top: 10px\" class=\"public_status\" v-on:click=\"status(mess.username,mess.id)\">\r\n                                        <div class=\"just-line-break\">\r\n                                            {{mess.message}}\r\n                                        </div>\r\n                                    </div>\r\n                                    <hr>\r\n                                </div>\r\n\r\n                            </div>\r\n                            <div v-if=\"edit==true\">\r\n                                <div style=\"display: flex\">\r\n                                    <img class=\"img_input_m\" :src=\"url+'/media/profile/'+data_mess.username+'/'+data_mess.username+'.png'\">\r\n                                        <textarea-autosize\r\n                                          class=\"input_mes\"\r\n                                          v-model=\"message\"\r\n                                          :min-height=\"30\"\r\n                                          :max-height=\"180\"\r\n                                         />\r\n\r\n                                </div>\r\n                                <div class=\"parent\">\r\n                                    <button class=\"button_style but_input_m child-active\" v-on:click=\"save_edit_mess()\">Сохранить</button>\r\n                                    <button class=\"button_style_cancel but_input_m \" v-on:click=\"cancel_edit_mess()\">Отмена</button>\r\n                                </div>\r\n                            </div>\r\n\r\n                        </div>\r\n                        <div class=\"right_w\">\r\n                            <div class=\"donate\">\r\n                                <p class=\"tags_style\">Донат</p>\r\n\r\n                                <p class=\"tags_style\">Закинь монет, брат</p>\r\n                            </div>\r\n                            <div style=\"height: 16px\"></div>\r\n                            <div class=\"tag\">\r\n                                <p class=\"tags_style\">Тэги</p>\r\n\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import Vue from 'vue'\r\n    import TextareaAutosize from 'vue-textarea-autosize'\r\n    import Header from \"./Header\"\r\n    import axios from \"axios\";\r\n\r\n    Vue.use(TextareaAutosize)\r\n\r\n    export default {\r\n        name: \"Status\",\r\n        components: {\r\n            Header\r\n        },\r\n        data() {\r\n            return {\r\n                url: this.$hostname,\r\n                id_url: null,\r\n                username_url: null,\r\n                access: false,\r\n                data_mess: {\r\n                    username: null,\r\n                    message: null,\r\n                    datetime: null,\r\n                    edit: null,\r\n                },\r\n                access_token: localStorage.getItem('access') || null,\r\n                edit: false,\r\n                message: null,\r\n                username: null,\r\n                comment: null,\r\n                all_comment: null,\r\n            }\r\n        },\r\n        methods: {\r\n\r\n            status(username,id){\r\n                var url_page = '/' + username + '/status/' + id;\r\n                this.$router.push(url_page);\r\n            },\r\n\r\n            get_comment(){\r\n                axios.post(this.url + '/api/mainpage/wall/get_comment/', {\r\n                    parent_id: this.id_url,\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.all_comment = res.data;\r\n                  })\r\n            },\r\n\r\n            save_comment(){\r\n                axios.post(this.url + '/api/mainpage/wall/save_comment/', {\r\n                    comment: this.comment,\r\n                    parent_id: this.id_url,\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.get_comment()\r\n                      this.comment = null\r\n                  })\r\n            },\r\n\r\n            delete_publ(){\r\n                axios.post(this.url + '/api/mainpage/wall/delete_mess/', {\r\n                    id_url: this.id_url,\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.back_home()\r\n                  })\r\n            },\r\n\r\n            cancel_edit_mess(){\r\n                this.edit = false;\r\n                this.message = null;\r\n            },\r\n\r\n            save_edit_mess(){\r\n                axios.post(this.url + '/api/mainpage/wall/save_edit_mess/', {\r\n                    message: this.message,\r\n                    id_url: this.id_url,\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                    location.reload()\r\n                  })\r\n            },\r\n\r\n            edit_publ(){\r\n              this.edit = true;\r\n              this.message = this.data_mess.message;\r\n            },\r\n\r\n            back_home(){\r\n                var url_page = '/' + this.data_mess.username;\r\n                this.$router.push(url_page);\r\n\r\n            },\r\n\r\n            go_page(username){\r\n                var url_page = '/' + username;\r\n                this.$router.push(url_page);\r\n\r\n            },\r\n\r\n            check_url_username(){\r\n                if (this.username_url != this.data_mess.username){\r\n                    var url_page = '/' + this.data_mess.username + '/status/' + this.id_url;\r\n                    this.$router.push(url_page);\r\n                }\r\n            },\r\n\r\n            check_id_and_username(){\r\n                var currentLocation = window.location + '';\r\n                let arr = new Array();\r\n                arr = currentLocation.split('/')\r\n                this.id_url = arr[6]\r\n                this.username_url = arr[4]\r\n                axios.post(this.url + '/api/mainpage/wall/get_mess_by_id/', {\r\n                    id_url: this.id_url,\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                    this.data_mess = res.data;\r\n                    this.check_url_username()\r\n                  })\r\n\r\n            },\r\n            user_check(){ //валидация пользователя из url\r\n              axios.post(this.url + '/api/mainpage/user_check/get_username/', {\r\n\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.username = res.data;\r\n                      if (this.username == this.data_mess.username){\r\n                          this.access = true;\r\n                      }\r\n                  })\r\n\r\n            },\r\n\r\n        },\r\n        created() {\r\n            this.check_id_and_username()\r\n            this.get_comment()\r\n            setTimeout(() => this.user_check(), 200);\r\n\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.parent {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  align-content: center;\r\n  height: 100%;\r\n  justify-content: flex-end;\r\n}\r\n.child-active {\r\n  flex-basis: 30%;\r\n}\r\n\r\n\r\n\r\n</style>"],"sourceRoot":"src/components/userpage"}]}