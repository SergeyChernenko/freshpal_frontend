       username_id: null,\r\n            },\r\n\r\n        }\r\n    },\r\n    props: {\r\n    },\r\n    methods: {\r\n\r\n        hashtag(){\r\n            var url_page = '/hashtag';\r\n            this.$router.push(url_page);\r\n        },\r\n\r\n        recommended(){\r\n            var url_page = '/recommended';\r\n            this.$router.push(url_page);\r\n        },\r\n\r\n        page(){\r\n            var url_page = '/' + this.username.username;\r\n            this.$router.push(url_page).catch(()=>{});\r\n        },\r\n\r\n        home(){\r\n            var url_page = '/subscriptions';\r\n            this.$router.push(url_page);\r\n        },\r\n\r\n        logout(){\r\n              localStorage.removeItem('access');\r\n              localStorage.removeItem('refresh');\r\n              this.$router.push('/')\r\n        },\r\n\r\n        user_check(){\r\n              axios.post(this.$hostname + '/api/mainpage/user_check/get_username/', {\r\n\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.username = res.data;\r\n                  })\r\n\r\n            },\r\n\r\n    },\r\n    created() {\r\n        this.user_check()\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.text{\r\n    font-size: 14pt; /* Размер текста */\r\n    font-weight: lighter; /* Светлое начертание */\r\n    cursor: pointer;\r\n    padding: 5px 0px 5px 20px;\r\n}\r\n\r\n.component_style{\r\n    cursor: pointer;\r\n}\r\n\r\n\r\n.component_style:hover {\r\n    background: #111111; /* Цвет фона */\r\n    border-radius: 30px;\r\n    /*text-decoration: none;*/\r\n}\r\n\r\n.a_text{\r\n    color: #fafafa;\r\n}\r\n.a_text:hover {\r\n    color: #fafafa;\r\n    text-decoration: none;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/components/Home"}]}