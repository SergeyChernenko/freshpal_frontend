A,GAAA,EAAA,EAAA;AACA,aAAA,WAAA,GAAA,KAAA,WAAA,CAAA,KAAA,CAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA;AACA,KAPA;AASA,IAAA,MATA,kBASA,KATA,EASA;AACA,WAAA,WAAA,IAAA,KAAA;AACA,KAXA;AAaA,IAAA,WAbA,yBAaA;AAAA;;AACA,UAAA,KAAA,gBAAA,IAAA,IAAA,EAAA;AACA,YAAA,KAAA,YAAA,IAAA,KAAA,kBAAA,EAAA;AACA,eAAA,gBAAA,GAAA,KAAA;AACA,SAFA,MAGA;AACA,eAAA,gBAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,yCAAA,EAAA;AACA,YAAA,QAAA,EAAA,KAAA;AADA,WAAA,EAGA;AACA,YAAA,OAAA,EAAA;AACA,+BAAA,YAAA,KAAA;AADA;AADA,WAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,YAAA,KAAA,CAAA,gBAAA,GAAA,GAAA,CAAA,IAAA;AACA,WAVA;AAWA;AAEA,OAnBA,MAmBA;AACA,QAAA,UAAA,CAAA,KAAA,OAAA,EAAA,GAAA,CAAA;AACA;AACA,KApCA;AAsCA,IAAA,UAtCA,wBAsCA;AAAA;;AACA,UAAA,KAAA,YAAA,IAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,wCAAA,EAAA;AACA,UAAA,QAAA,EAAA,KAAA;AADA,SAAA,EAGA;AACA,UAAA,OAAA,EAAA;AACA,6BAAA,YAAA,KAAA;AADA;AADA,SAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,gBAAA,GAAA,GAAA,CAAA,IAAA;;AACA,UAAA,MAAA,CAAA,WAAA;AACA,SAXA;AAYA;AACA,KArDA;AAuDA,IAAA,aAvDA,2BAuDA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,yCAAA,EAAA;AACA,QAAA,MAAA,EAAA,KAAA;AADA,OAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA,CACA,CATA;AAUA,KAlEA;AAoEA,IAAA,QApEA,sBAoEA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,+CAAA,EAAA,EAAA,EAEA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAFA,EAOA,IAPA,CAOA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,WAAA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,MAAA,IAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,QAAA;AACA;;AACA,YAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,IAAA,IAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,CAAA,IAAA,CAAA,MAAA;AACA;AACA,OAfA;AAgBA,KArFA;AAuFA,IAAA,gBAvFA,8BAuFA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,8CAAA,EAAA;AACA,QAAA,WAAA,EAAA,KAAA;AADA,OAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAVA;AAWA;AAnGA,GA/BA;AAqIA,EAAA,OArIA,qBAqIA;AACA,IAAA,QAAA,CAAA,KAAA,GAAA,WAAA;AACA,SAAA,QAAA;AACA;AAxIA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <p class=\"p_dis\">Добавить описание</p>\r\n        <div class=\"wrapper_input\">\r\n            <textarea-autosize\r\n              placeholder=\"Расскажите о себе...\"\r\n              class=\"input_mes\"\r\n              v-model=\"description\"\r\n              :maxlength =\"limit\"\r\n              :min-height=\"30\"\r\n              :max-height=\"250\"\r\n              v-on:input=\"lineCount\"\r\n             />\r\n            <emoji-picker @emoji=\"append\" :search=\"emoji_mes\">\r\n              <button\r\n                class=\"emoji-invoker\"\r\n                slot=\"emoji-invoker\"\r\n                slot-scope=\"{ events: { click: clickEvent } }\"\r\n                @click.stop=\"clickEvent\"\r\n              >\r\n\r\n                    <p><span class=\"select\">&#128512;</span></p>\r\n\r\n              </button>\r\n\r\n              <div slot=\"emoji-picker\" slot-scope=\"{ emojis, insert, display }\">\r\n                <div class=\"emoji-picker\" :style=\"{left: display.x-680 + 'px'}\">\r\n                  <div style=\"margin-bottom: 10px\" class=\"emoji-picker__search\">\r\n                    <input class=\"input_style\"  type=\"text\" v-model=\"emoji_mes\" v-focus>\r\n                  </div>\r\n                  <div>\r\n                    <div v-for=\"(emojiGroup, category) in emojis\" :key=\"category\">\r\n                      <h5>{{ category }}</h5>\r\n                      <div class=\"emojis\">\r\n                        <span\r\n                          v-for=\"(emoji, emojiName) in emojiGroup\"\r\n                          :key=\"emojiName\"\r\n                          @click=\"insert(emoji)\"\r\n                          :title=\"emojiName\"\r\n                        >{{ emoji }}</span>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </emoji-picker>\r\n        </div>\r\n        <div style=\"display: flex\">\r\n            <div style=\"flex-grow: 1;\"></div>\r\n            <div style=\"margin: 15px 15px 0px 0px\">\r\n                <span class=\"message-counter\">{{ description.length}} / {{limit}}</span>\r\n            </div>\r\n            <span v-if=\"result_des == true\" style=\"margin: 15px 10px 0px 0px; color: #40ff40;\">Сохранено</span>\r\n            <button class=\"button_style child-active\" v-on:click=\"save_description\">Сохранить</button>\r\n        </div>\r\n        <hr>\r\n        <p class=\"p_dis\">Фильтр публикаций</p>\r\n        Публикации могут содержать контент, не предназначенный для большинства. А именно нецензурные слова и материалы 18+.\r\n        Данные публикации не появляются в ленте и в популярных за месяц. Но это можно изменить, настроив фильтр.\r\n        <label class=\"container\"><div style=\"margin-left: 15px\">Нецензурные слова</div>\r\n          <input type=\"checkbox\" checked=\"checked\" class=\"check-highload\" id=\"censor\" value=\"censor\" v-model=\"filter\" v-on:change=\"filter_censor\">\r\n          <span class=\"highload2\"></span>\r\n        </label>\r\n        <label class=\"container\"><div style=\"margin-left: 15px\">Материалы 18+</div>\r\n          <input type=\"checkbox\" checked=\"checked\" class=\"check-highload\" id=\"nude\" value=\"nude\"  v-model=\"filter\" v-on:change=\"filter_censor\">\r\n          <span class=\"highload2\"></span>\r\n        </label>\r\n        <hr>\r\n        <p class=\"p_dis\">Изменить пароль</p>\r\n        <div style=\"width: 100%\">\r\n            <div class=\"center\">\r\n                <form @submit.prevent=\"check_pass\">\r\n                    <p><input class=\"input_style\" id=\"old_password\" placeholder=\"Старый пароль\" type=\"password\" v-model=\"old_password\" name=\"password\" required minlength=\"8\" pattern=\"(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{8,}\"></p>\r\n                    <p><input class=\"input_style\" id=\"new_password\" placeholder=\"Новый пароль\" type=\"password\" v-model=\"new_password\" name=\"password\" required minlength=\"8\" pattern=\"(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{8,}\"></p>\r\n                    <p><input class=\"input_style\" id=\"new_password_again\" placeholder=\"Подтверждение пароля\" type=\"password\" v-model=\"new_password_again\" name=\"password\" required minlength=\"8\" pattern=\"(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{8,}\"></p>\r\n                    <p><button class=\"button_style\" type=\"submit\">Сохранить</button></p>\r\n                </form>\r\n                <p v-if=\"res_old_password == false\">Старый пароль не совпадает</p>\r\n                <p v-if=\"res_new_password == false\">Новые пароли не совпадают</p>\r\n                <p v-if=\"res_new_password == true\">Пароль изменен</p>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\nimport EmojiPicker from 'vue-emoji-picker'\r\nimport Vue from \"vue\";\r\nVue.use(EmojiPicker)\r\n\r\nexport default {\r\n    name: \"Settings\",\r\n    components: {\r\n        EmojiPicker\r\n\r\n    },\r\n    data() {\r\n        return {\r\n            access_token: localStorage.getItem('access') || null,\r\n            description: '',\r\n            result_des: false,\r\n            filter: [],\r\n            old_password: null,\r\n            new_password: null,\r\n            new_password_again: null,\r\n            res_old_password : null,\r\n            res_new_password: null,\r\n            emoji_mes: '',\r\n            limit: 400,\r\n        }\r\n    },\r\n    props: {\r\n        username: Object,\r\n    },\r\n    directives: {\r\n        focus: {\r\n          inserted(el) {\r\n            el.focus()\r\n          },\r\n        },\r\n      },\r\n    methods: {\r\n\r\n        lineCount() {\r\n             var line =  this.description.length ? this.description.split(/\\r\\n|\\r|\\n/).length : 0\r\n             if(line > 15){\r\n                 this.description = this.description.slice(0, -1);\r\n             }\r\n        },\r\n\r\n        append(emoji) {\r\n          this.description += emoji\r\n        },\r\n\r\n        change_pass(){\r\n            if ( this.res_old_password == true ) {\r\n                if(this.new_password != this.new_password_again){\r\n                    this.res_new_password = false\r\n                }\r\n                else{\r\n                    this.res_new_password = null\r\n                    axios.post(this.$hostname + '/api/mainpage/user_profile/change_pass/', {\r\n                        password: this.new_password\r\n                      },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                    })\r\n                      .then(res => {\r\n                          this.res_new_password = res.data\r\n                  })\r\n                }\r\n\r\n            } else {\r\n                setTimeout( this.timeout, 100);\r\n            }\r\n        },\r\n\r\n        check_pass(){\r\n            if (this.old_password != null){\r\n                axios.post(this.$hostname + '/api/mainpage/user_profile/check_pass/', {\r\n                    password: this.old_password\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.res_old_password = res.data\r\n                      this.change_pass()\r\n              })\r\n          }\r\n        },\r\n\r\n        filter_censor(){\r\n          axios.post(this.$hostname + '/api/mainpage/user_profile/edit_censor/', {\r\n              filter: this.filter\r\n              },\r\n          {\r\n                headers: {\r\n                    'Authorization': 'Bearer ' + this.access_token,\r\n                }\r\n            })\r\n              .then(res => {\r\n          })\r\n        },\r\n\r\n        get_data(){\r\n            axios.post(this.$hostname + '/api/mainpage/user_profile/get_data_settings/', {\r\n              },\r\n          {\r\n                headers: {\r\n                    'Authorization': 'Bearer ' + this.access_token,\r\n                }\r\n            })\r\n              .then(res => {\r\n                this.description = res.data[0].description\r\n                if (res.data[0].censor == true){\r\n                    this.filter.push('censor')\r\n                }\r\n                if (res.data[0].nude == true){\r\n                    this.filter.push('nude')\r\n                }\r\n              })\r\n        },\r\n\r\n        save_description(){\r\n            axios.post(this.$hostname + '/api/mainpage/user_profile/description_save/', {\r\n                description: this.description\r\n              },\r\n          {\r\n                headers: {\r\n                    'Authorization': 'Bearer ' + this.access_token,\r\n                }\r\n            })\r\n              .then(res => {\r\n                this.result_des = res.data\r\n              })\r\n        }\r\n\r\n    },\r\n    created() {\r\n        document.title = 'Настройки'\r\n        this.get_data()\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.p_dis{\r\n    font-family: 'Open Sans', sans-serif;\r\n   font-size: 18px; /* Размер текста */\r\n    text-align: center;\r\n    color: #fafafa;\r\n    margin-top:10px;\r\n}\r\n.child-active {\r\n    margin-top: 5px;\r\n  flex-basis: 22%;\r\n}\r\n\r\n\r\n\r\n\r\n.center {\r\n  margin: auto;\r\n  width: 300px;\r\n\r\n}\r\n\r\n</style>"],"sourceRoot":"src/components/Other"}]}