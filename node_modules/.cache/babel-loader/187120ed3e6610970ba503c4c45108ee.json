    <div class=\"itm_1_1\"></div>-->\r\n                <image-uploader\r\n                    :maxWidth=\"700\"\r\n                    :maxHeight=\"700\"\r\n                    :quality=\"1\"\r\n                    :className=\"['fileInput', { 'fileinput--loaded': hasImage }]\"\r\n                    doNotResize=\"['gif', 'svg']\"\r\n                    outputFormat=\"string\"\r\n                    ref=\"avatarGet\"\r\n                    :preview=false\r\n                    @input=\"setImage\"\r\n                >\r\n                </image-uploader>\r\n\r\n                <div class=\"avatar_click\">\r\n                    <img style=\"border-radius: 50%\" :src=\"url+'/media/profile/'+this.username_url+'/'+this.username_url+'.png'\">\r\n                    <label v-if=\"access==true\" for=\"fileInput\"><div id='invisible'></div></label>\r\n                </div>\r\n\r\n                <div class=\"itm_1_2\"></div>\r\n                <div>\r\n                    <div class=\"itm_1_3\">\r\n                        <div><p class=\"username_style\"> {{ this.username_url }}</p></div>\r\n                        <div style=\"width: 30px;\"></div>\r\n                        <div><p align=\"left\"><font size=\"6\" color=\"#2fcf72\" face=\"Comic Sans MS\"> 251</font></p></div>\r\n                        <div style=\"width: 80px;\"></div>\r\n                        <div>\r\n                            <button v-if=\"access != true && access_token != null && check_sub_res == false\" class=\"button_style\" v-on:click=\"subscribe()\">Подписаться</button>\r\n                            <button v-if=\"access != true && access_token != null && check_sub_res == true\" class=\"button_style\" v-on:click=\"unsubscribe()\">Отписаться</button>\r\n                        </div>\r\n<!--                        <p v-if=\"access == true\" class=\"text_style\"><a class=\"a_style\" v-on:click=\"edit_profile\">Редактировать страницу</a></p>-->\r\n                    </div>\r\n                    <div class=\"itm_1_3\">\r\n                    <p class=\"text_style\">0 публикации </p>\r\n                    <div class=\"distance_sub\"></div>\r\n                    <p class=\"text_style\"><a class=\"sub_link\" v-on:click=\"followers_show()\">{{sum_sub[0]}} подписчиков</a></p>\r\n                    <div class=\"distance_sub\"></div>\r\n                    <p class=\"text_style\"><a class=\"sub_link\" v-on:click=\"following_show()\">{{sum_sub[1]}} подписок</a></p>\r\n                </div>\r\n\r\n                </div>\r\n            </div>\r\n        <hr>\r\n        </div>\r\n\r\n    </div>\r\n\r\n    <div class=\"background_main\">\r\n        <div class=\"center\" v-if=\"username_url_check == false\">\r\n            <div class=\"center_text\">\r\n                <div style=\"height: 100px\"></div>\r\n                <p class=\"text_style_2\">Пользователя {{ this.username_url }} не существует</p>\r\n                <div v-if=\"access_token == null\">\r\n                    <p class=\"text_style_2\"><a class=\"a_style\" v-on:click=\"create_new_page()\">Создать страницу</a></p>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n   <div class=\"no_user\" v-if=\"username_url_check == null\">\r\n   </div>\r\n\r\n    <modal name=\"followers\"\r\n       :width=\"420\"\r\n       :height=\"480\"\r\n       :pivot-y=\"0.5\"\r\n       :adaptive=\"false\"\r\n       :scrollable=\"true\"\r\n       styles=\"border-radius: 17px; background: #444444; box-shadow: 0 0 1px 1px #5D5D5D\"\r\n       transition=\"nice-modal-fade\">\r\n        <p class=\"text_style_2\">Подписчики</p>\r\n        <hr>\r\n        <div v-for=\"follower in list_followers\">\r\n            <div class=\"list_modal_style\">\r\n                <div class=\"body_sub\">\r\n                   <img class=\"img_sub\" :src=\"url+'/media/profile/'+follower+'/'+follower+'.png'\">\r\n                   <div style=\"width: 10px\"></div>\r\n                   <p class=\"text_style\"><a class=\"sub_link\" :href=\"'#/'+follower\">{{follower}}</a></p>\r\n                   <div style=\"width: 200px\"></div>\r\n                    <button class=\"button_style\" v-on:click=\"subscribe()\">Подписаться</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </modal>\r\n\r\n    <modal name=\"following\"\r\n       :width=\"420\"\r\n       :height=\"480\"\r\n       :pivot-y=\"0.5\"\r\n       :adaptive=\"true\"\r\n       :scrollable=\"true\"\r\n       styles=\"border-radius: 17px; background: #444444; box-shadow: 0 0 1px 1px #5D5D5D\"\r\n       transition=\"nice-modal-fade\">\r\n       <p class=\"text_style_2\">Подписки</p>\r\n        <hr>\r\n        <div v-for=\"following in list_following\">\r\n            <div class=\"list_modal_style\">\r\n                <div class=\"body_sub\">\r\n                   <img class=\"img_sub\" :src=\"url+'/media/profile/'+following+'/'+following+'.png'\">\r\n\r\n                   <p class=\"text_style\"><a class=\"sub_link\" :href=\"'#/'+following\">{{following}}</a></p>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </modal>\r\n\r\n    <modal name=\"edit_profile\"\r\n       :width=\"420\"\r\n       :height=\"480\"\r\n       :pivot-y=\"0.5\"\r\n       :adaptive=\"true\"\r\n       :scrollable=\"true\"\r\n       styles=\"border-radius: 17px; background: #444444; box-shadow: 0 0 1px 1px #5D5D5D\"\r\n       transition=\"nice-modal-fade\">\r\n       <label for=\"fileInput\"><p><a class=\"sub_link\">Изменить фото профиля</a></p></label>\r\n    </modal>\r\n\r\n     <modal name=\"crop_avatar\"\r\n       :width=\"470\"\r\n       height=\"auto\"\r\n       :pivot-y=\"0.5\"\r\n       :adaptive=\"true\"\r\n       :scrollable=\"true\"\r\n\r\n       styles=\"border-radius: 17px; background: #272727; box-shadow: 0 0 1px 1px #5D5D5D\"\r\n       transition=\"nice-modal-fade\">\r\n       <div style=\"display: flex\">\r\n         <cropper\r\n\t\t\tref=\"cropper\"\r\n\t\t\tclass=\"avatat-result__cropper\"\r\n\t\t\t:src=\"url_avatar\"\r\n\t\t\t:debounce=\"false\"\r\n\t\t\t:stencil-props=\"{\r\n\t\t\t\taspectRatio: 1,\r\n\r\n\t\t\t}\"\r\n            :stencil-component=\"$options.components.CircleStencil\"\r\n\t\t\t@change=\"onChange\"\r\n\t\t/>\r\n\t\t<div class=\"avatat-result__previews\">\r\n\t\t\t<preview style=\"\" class=\"avatat-result__preview\" :image=\"result.image\" :coordinates=\"result.coordinates\" />\r\n            <div style=\"height: 15px\"></div>\r\n            <label for=\"fileInput\"><p><a class=\"sub_link\">Изменить</a></p></label>\r\n\t\t</div>\r\n\r\n        </div>\r\n\r\n        <div class=\"button_avatar\">\r\n            <button class=\"button_style\" v-on:click=\"onCrop\">Сохранить</button>\r\n            <div style=\"width: 25px\"></div>\r\n            <button class=\"button_style\" v-on:click=\"crop_avatar_hide\">Выйти</button>\r\n        </div>\r\n\r\n    </modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport axios from 'axios';\r\nimport VModal from 'vue-js-modal';\r\nimport  Vue from  'vue';\r\nimport { CircleStencil, Cropper, Preview  } from 'vue-advanced-cropper';\r\nimport 'vue-advanced-cropper/dist/style.css';\r\nimport ImageUploader from 'vue-image-upload-resize'\r\nimport Header from \"./Header\"\r\n\r\n\r\n\r\n\r\n\r\nVue.use(ImageUploader);\r\nVue.use(VModal);\r\n// Vue.forceUpdate();\r\n\r\nexport default {\r\n  name: 'startpage',\r\n  components: {\r\n      Cropper, CircleStencil, Preview,\r\n      ImageUploader,\r\n      Header\r\n  },\r\n\r\n  data(){\r\n      return{\r\n        url: this.$hostname,\r\n        access_token: localStorage.getItem('access') || null,\r\n        refresh_token: localStorage.getItem('refresh') || null,\r\n        username_url: this.$route.params.username,\r\n        username: null,\r\n        access: null,\r\n        username_url_check: null,\r\n        sum_sub: Array,\r\n        check_sub_res: null,\r\n        selected_avatar: null,\r\n        url_avatar: null,\r\n        list_followers: Array,\r\n        list_following: Array,\r\n        result: {\r\n\t\t\t\tcoordinates: null,\r\n\t\t\t\timage: null,\r\n\t\t\t},\r\n        hasImage: false,\r\n        res_avatar: null,\r\n      }\r\n  },\r\n\r\n  methods: {\r\n\r\n\r\n\r\n    edit_profile(){\r\n        this.$modal.show('edit_profile');\r\n    },\r\n\r\n    setImage(output) {\r\n      this.hasImage = true;\r\n      this.url_avatar = output;\r\n      this.crop_avatar_show()\r\n    },\r\n\r\n    onChange({ coordinates, image }) {\r\n\r\n        this.result = {\r\n            coordinates,\r\n            image,\r\n        };\r\n    },\r\n\r\n    onCrop() {\r\n        const result = this.$refs.cropper.getResult().canvas.toDataURL();;\r\n        // console.log(result)\r\n        axios.post(this.url + '/api/mainpage/avatar/set/', {\r\n            avatar: result\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.res_avatar = res.data;\r\n              location.reload();\r\n          });\r\n\r\n    },\r\n\r\n    // avatar_selected(event){\r\n    //     this.url_avatar = null;\r\n    //     this.selectedAvatar = event.target.files[0];\r\n    //     this.url_avatar = URL.createObjectURL(this.selectedAvatar);\r\n    //     console.log(this.url_avatar)\r\n    //     this.crop_avatar_show()\r\n    //\r\n    // },\r\n    crop_avatar_show(){\r\n\r\n        this.$modal.show('crop_avatar');\r\n    },\r\n    crop_avatar_hide(){\r\n        location.reload();\r\n        this.$modal.hide('crop_avatar');\r\n    },\r\n\r\n    followers_show(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/list_followers/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.list_followers = res.data;\r\n          });\r\n        this.$modal.show('followers');\r\n    },\r\n    // followers_hide () {\r\n    //     this.$modal.hide('followers');\r\n    // },\r\n    following_show(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/list_following/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n            headers: {\r\n                'Authorization': 'Bearer ' + this.access_token,\r\n            }\r\n        })\r\n          .then(res => {\r\n              this.list_following = res.data;\r\n          })\r\n        this.$modal.show('following');\r\n    },\r\n\r\n    check_sub(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/check_sub/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data;\r\n          })\r\n    },\r\n\r\n    get_sum_sub(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/sum/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.sum_sub = res.data;\r\n          })\r\n    },\r\n\r\n    subscribe(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/plus/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data\r\n              this.get_sum_sub()\r\n          })\r\n    },\r\n\r\n    unsubscribe(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/minus/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n 