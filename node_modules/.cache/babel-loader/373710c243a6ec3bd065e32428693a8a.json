DA,OAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAVA;AAWA,KAzHA;AA2HA,IAAA,SA3HA,uBA2HA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,+BAAA,EAAA;AACA,QAAA,QAAA,EAAA,KAAA;AADA,OAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAVA;AAWA,KAvIA;AAyIA,IAAA,WAzIA,yBAyIA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,gCAAA,EAAA;AACA,QAAA,QAAA,EAAA,KAAA;AADA,OAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,aAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAVA;AAWA,KArJA;AAuJA,IAAA,SAvJA,uBAuJA;AACA,UAAA,eAAA,GAAA,MAAA,CAAA,QAAA,GAAA,EAAA;AACA,UAAA,GAAA,GAAA,IAAA,KAAA,EAAA;AACA,MAAA,GAAA,GAAA,eAAA,CAAA,KAAA,CAAA,GAAA,CAAA;;AACA,UAAA,GAAA,CAAA,CAAA,CAAA,IAAA,KAAA,QAAA,EACA;AACA,YAAA,QAAA,GAAA,MAAA,KAAA,QAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AACA,KAhKA;AAkKA,IAAA,eAlKA,6BAkKA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,KApKA;AAsKA,IAAA,cAtKA,4BAsKA;AAAA;;AAEA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,qCAAA,EAAA;AACA,QAAA,QAAA,EAAA,KAAA;AADA,OAAA,EAGA,IAHA,CAGA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,kBAAA,GAAA,GAAA,CAAA,IAAA;AAEA,OANA;AAQA,KAhLA;AAkLA,IAAA,eAlLA,6BAkLA;AACA,WAAA,SAAA;AACA,KApLA;AAsLA,IAAA,UAtLA,wBAsLA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,wCAAA,EAAA,EAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,eAAA;;AACA,YAAA,MAAA,CAAA,QAAA,IAAA,MAAA,CAAA,YAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA;AACA,OAdA;AAgBA,KAvMA;AAyMA,IAAA,MAzMA,oBAyMA;AACA,MAAA,YAAA,CAAA,UAAA,CAAA,QAAA;AACA,MAAA,YAAA,CAAA,UAAA,CAAA,SAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,WAAA,aAAA,GAAA,IAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AA/MA,GA5BA;AAgPA,EAAA,KAAA,EAAA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,IAAA,EAAA;AADA,KADA;AAIA,IAAA,WAAA,EAAA;AACA,MAAA,IAAA,EAAA;AADA,KAJA;AAOA,IAAA,WAAA,EAAA;AACA,MAAA,IAAA,EAAA;AADA,KAPA;AAUA,IAAA,kBAAA,EAAA;AACA,MAAA,IAAA,EAAA;AADA;AAVA,GAhPA;AA+PA,EAAA,QAAA,EAAA;AACA,IAAA,KADA,mBACA;AAAA,kCACA,KAAA,kBADA;AAAA,UACA,MADA,yBACA,MADA;AAAA,UACA,KADA,yBACA,KADA;AAAA,UACA,IADA,yBACA,IADA;AAAA,UACA,GADA,yBACA,GADA;AAEA,UAAA,KAAA,GAAA;AACA,QAAA,KAAA,YAAA,KAAA,OADA;AAEA,QAAA,MAAA,YAAA,MAAA,OAFA;AAGA,QAAA,SAAA,sBAAA,IAAA,iBAAA,GAAA;AAHA,OAAA;;AAKA,UAAA,KAAA,WAAA,IAAA,KAAA,WAAA,CAAA,OAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,aAAA,KAAA,WAAA,CAAA,IAAA,gBAAA,KAAA,WAAA,CAAA,cAAA;AACA;;AACA,aAAA,KAAA;AACA;AAZA,GA/PA;AA8QA,EAAA,OA9QA,qBA8QA;AACA,SAAA,cAAA,GADA,CACA;;AACA,SAAA,WAAA,GAFA,CAEA;;AACA,SAAA,YAAA,GAAA,YAAA,CAAA,OAAA,CAAA,QAAA,CAAA;AACA,SAAA,aAAA,GAAA,YAAA,CAAA,OAAA,CAAA,SAAA,CAAA;;AACA,QAAA,KAAA,YAAA,IAAA,IAAA,EAAA;AACA,WAAA,UAAA;AACA;AACA;AAtRA,CAAA","sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <header class=\"header\">\r\n      <div class=\"flex\">\r\n          <div class=\"it_indent\"></div>\r\n          <div class=\"itm_logo\"><img width=\"120\" height=\"40\" src=\"http://127.0.0.1:8000/media/logo.svg\"></div>\r\n          <div style=\"flex-basis: 40px\"></div>\r\n          <div class=\"itm_search\"><input class=\"search\" type=\"search\" name=\"search\" placeholder=\"Поиск\"></div>\r\n          <div class=\"itm_2\"></div>\r\n          <button type=\"submit\" v-on:click=\"back_home()\">{{username}}</button>\r\n          <button type=\"submit\" v-on:click=\"logout()\">Выйти</button>\r\n          <div class=\"it_indent\"></div>\r\n      </div>\r\n\t</header><!-- .header-->\r\n    <div v-if=\"username_url_check == true\">\r\n        <div class=\"mainitem\">\r\n            <div class=\"sideitem_1\">\r\n                <div class=\"itm_1_1\"></div>\r\n<!--                <input style=\"display: none\" type=\"file\" @change=\"avatar_selected\" ref=\"avatarInput\">-->\r\n<!--                <button @click=\"$refs.avatarInput.click()\">Load</button>-->\r\n<!--                <button @click=\"upload_avatar\">Upload</button>-->\r\n\r\n                <img width=\"150\" height=\"150\" src=\"http://127.0.0.1:8000/media/main_photo.png\" v-on:click=\"crop_avatar_show\">\r\n                <div class=\"itm_1_2\"></div>\r\n                <div class=\"itm_1_3\">\r\n\r\n                    <div><p align=\"left\"><font size=\"6\" color=\"#fcfcfc\" face=\"Trebuchet MS\"> {{ this.username_url }}</font></p></div>\r\n                    <div><p align=\"left\"><font size=\"6\" color=\"#2fcf72\" face=\"Comic Sans MS\"> 251</font></p></div>\r\n                </div>\r\n                <div class=\"itm_1_2\"></div>\r\n                <div class=\"itm_1_3\">\r\n                    <p align=\"left\"><font size=\"4\" color=\"#fcfcfc\" face=\"Trebuchet MS\">Публикации - 0</font></p>\r\n                    <p align=\"left\"><a class=\"a_style\" v-on:click=\"followers_show()\"><font size=\"4\" color=\"#fcfcfc\" face=\"Trebuchet MS\">Подписчики - {{sum_sub[0]}}</font></a></p>\r\n                    <p align=\"left\"><a class=\"a_style\" v-on:click=\"following_show()\"><font size=\"4\" color=\"#fcfcfc\" face=\"Trebuchet MS\">Подписки - {{sum_sub[1]}}</font></a></p>\r\n                </div>\r\n                <div class=\"itm_1_2\"></div>\r\n                <div class=\"itm_1_3\">\r\n                    <p><a class=\"a_style\" v-on:click=\"\">Редактировать страницу</a></p>\r\n                    <button v-if=\"access != true && access_token != null && check_sub_res == false\" class=\"button_style\" v-on:click=\"subscribe()\">Подписаться</button>\r\n                    <button v-if=\"access != true && access_token != null && check_sub_res == true\" class=\"button_style\" v-on:click=\"unsubscribe()\">Отписаться</button>\r\n                </div>\r\n            </div>\r\n\r\n            <p>\r\n\r\n            </p>\r\n        </div>\r\n    </div>\r\n    <div class=\"mainitem\" v-if=\"username_url_check == false\">\r\n        <div class=\"center_text\">\r\n            <div style=\"height: 100px\"></div>\r\n            <p style=\"text-align:  center\" align=\"left\"><font size=\"4\" color=\"#fcfcfc\" face=\"Trebuchet MS\">Пользователя {{ this.username_url }} не существует.</font></p>\r\n            <div v-if=\"access_token == null\">\r\n                <p style=\"text-align:  center\" align=\"left\"><a class=\"a_style\" v-on:click=\"create_new_page()\">Создать страницу</a></p>\r\n            </div>\r\n        </div>\r\n    </div>\r\n   <div class=\"mainitem\" v-if=\"username_url_check == null\">\r\n    </div>\r\n\r\n    <modal name=\"followers\"\r\n       :width=\"420\"\r\n       :height=\"480\"\r\n       :pivot-y=\"0.5\"\r\n       :adaptive=\"false\"\r\n       :scrollable=\"true\"\r\n       styles=\"border-radius: 17px; background: #444444; box-shadow: 0 0 1px 1px #5D5D5D\"\r\n       transition=\"nice-modal-fade\">\r\n        <font size=\"5\" color=\"#fcfcfc\" face=\"Trebuchet MS\">Подписчики</font>\r\n        <hr>\r\n        <div v-for=\"follower in list_followers\">\r\n            <div class=\"list_modal_style\">\r\n                <p><a class=\"a_style\" :href=\"'#/'+follower\"><font size=\"4\" color=\"#fcfcfc\" face=\"Trebuchet MS\">{{follower}}</font></a></p>\r\n            </div>\r\n        </div>\r\n    </modal>\r\n\r\n    <modal name=\"following\"\r\n       :width=\"420\"\r\n       :height=\"480\"\r\n       :pivot-y=\"0.5\"\r\n       :adaptive=\"true\"\r\n       :scrollable=\"true\"\r\n       styles=\"border-radius: 17px; background: #444444; box-shadow: 0 0 1px 1px #5D5D5D\"\r\n       transition=\"nice-modal-fade\">\r\n       <font size=\"5\" color=\"#fcfcfc\" face=\"Trebuchet MS\">Подписки</font>\r\n        <hr>\r\n        <div v-for=\"following in list_following\">\r\n            <div class=\"list_modal_style\">\r\n                <p><a class=\"a_style\" :href=\"'#/'+following\"><font size=\"4\" color=\"#fcfcfc\" face=\"Trebuchet MS\">{{following}}</font></a></p>\r\n            </div>\r\n        </div>\r\n    </modal>\r\n\r\n     <modal name=\"crop_avatar\"\r\n       :width=\"600\"\r\n       :height=\"600\"\r\n       :pivot-y=\"0.5\"\r\n       :adaptive=\"true\"\r\n       :scrollable=\"true\"\r\n       styles=\"border-radius: 17px; background: #444444; box-shadow: 0 0 1px 1px #5D5D5D\"\r\n       transition=\"nice-modal-fade\">\r\n\r\n       <div class=\"circle-stencil\" :style=\"style\">\r\n        <draggable-element\r\n          class=\"circle-stencil__handler\"\r\n          @drag=\"onResize\"\r\n          @drag-end=\"onResizeEnd\"\r\n        >\r\n          <svg\r\n            class=\"circle-stencil__icon\"\r\n            xmlns=\"http://www.w3.org/2000/svg\"\r\n            width=\"26.7\"\r\n            height=\"26.3\"\r\n          >\r\n            <path\r\n              fill=\"#FFF\"\r\n              d=\"M15.1 4.7L18.3 6l-3.2 3.3 2.3 2.3 3.3-3.3 1.3 3.3L26.7 0zM9.3 14.7L6 18l-1.3-3.3L0 26.3l11.6-4.7-3.3-1.3 3.3-3.3z\"\r\n            ></path>\r\n          </svg>\r\n        </draggable-element>\r\n        <draggable-area @move=\"onMove\" @move-end=\"onMoveEnd\">\r\n          <stencil-preview\r\n            class=\"circle-stencil__preview\"\r\n            :image=\"image\"\r\n            :coordinates=\"coordinates\"\r\n            :width=\"stencilCoordinates.width\"\r\n            :height=\"stencilCoordinates.height\"\r\n            :transitions=\"transitions\"\r\n          />\r\n        </draggable-area>\r\n      </div>\r\n\r\n    </modal>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport axios from 'axios';\r\nimport VModal from 'vue-js-modal';\r\nimport  Vue from  'vue';\r\nimport {\r\n  DraggableElement,\r\n  DraggableArea,\r\n  StencilPreview,\r\n  ResizeEvent,\r\n} from \"vue-advanced-cropper\";\r\n\r\nVue.use(VModal);\r\n\r\nexport default {\r\n  name: 'startpage',\r\n  components: {\r\n    StencilPreview,\r\n    DraggableArea,\r\n    DraggableElement,\r\n  },\r\n\r\n  data(){\r\n      return{\r\n        url: 'http://127.0.0.1:8000',\r\n        access_token: localStorage.getItem('access') || null,\r\n        refresh_token: localStorage.getItem('refresh') || null,\r\n        username_url: this.$route.params.username,\r\n        username: null,\r\n        access: null,\r\n        username_url_check: null,\r\n        sum_sub: Array,\r\n        check_sub_res: null,\r\n        selected_avatar: null,\r\n        url_avatar: null,\r\n        list_followers: Array,\r\n        list_following: Array,\r\n\r\n        img: 'https://images.unsplash.com/photo-1600984575359-310ae7b6bdf2?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=700&q=80'\r\n      }\r\n  },\r\n\r\n  methods: {\r\n\r\n    onMove(moveEvent) {\r\n      this.$emit(\"move\", moveEvent);\r\n    },\r\n    onMoveEnd() {\r\n      this.$emit(\"move-end\");\r\n    },\r\n    onResize(dragEvent) {\r\n      const shift = dragEvent.shift();\r\n\r\n      const widthResize = shift.left;\r\n      const heightResize = -shift.top;\r\n\r\n      this.$emit(\r\n        \"resize\",\r\n        new ResizeEvent(\r\n          {\r\n            left: widthResize,\r\n            right: widthResize,\r\n            top: heightResize,\r\n            bottom: heightResize,\r\n          },\r\n          {\r\n            compensate: true,\r\n          }\r\n        )\r\n      );\r\n    },\r\n    onResizeEnd() {\r\n      this.$emit(\"resize-end\");\r\n    },\r\n    aspectRatios() {\r\n      return {\r\n        minimum: 1,\r\n        maximum: 1,\r\n      };\r\n    },\r\n\r\n    change({ coordinates, canvas }) {\r\n\t\t\tconsole.log(coordinates, canvas)\r\n\t\t},\r\n\r\n    send_url(name){\r\n        var url_page = '/' + name;\r\n        this.$router.push(url_page);\r\n    },\r\n\r\n    // avatar_selected(event){\r\n    //     this.selectedAvatar = event.target.files[0];\r\n    //     this.url_avatar = URL.createObjectURL(this.selectedAvatar);\r\n    //     console.log(this.url_avatar)\r\n    // },\r\n    //\r\n    // upload_avatar() {\r\n    //\r\n    // },\r\n\r\n    crop_avatar_show(){\r\n        this.$modal.show('crop_avatar');\r\n    },\r\n\r\n    followers_show(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/list_followers/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.list_followers = res.data;\r\n          })\r\n        this.$modal.show('followers');\r\n    },\r\n    // followers_hide () {\r\n    //     this.$modal.hide('followers');\r\n    // },\r\n    following_show(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/list_following/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n            headers: {\r\n                'Authorization': 'Bearer ' + this.access_token,\r\n            }\r\n        })\r\n          .then(res => {\r\n              this.list_following = res.data;\r\n              console.log(this.list_following)\r\n          })\r\n        this.$modal.show('following');\r\n    },\r\n\r\n    check_sub(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/check_sub/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data;\r\n          })\r\n    },\r\n\r\n    get_sum_sub(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/sum/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.sum_sub = res.data;\r\n          })\r\n    },\r\n\r\n    subscribe(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/plus/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data\r\n          })\r\n    },\r\n\r\n    unsubscribe(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/minus/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data;\r\n          })\r\n    },\r\n\r\n    back_home(){\r\n        var currentLocation = window.location + '';\r\n        let arr = new Array();\r\n        arr = currentLocation.split('/')\r\n        if (arr[4] != this.username)\r\n        {\r\n            var url_page = '/' + this.username;\r\n            this.$router.push(url_page);\r\n        }\r\n    },\r\n\r\n    create_new_page(){\r\n        this.$router.push('/')\r\n    },\r\n\r\n    user_url_check(){\r\n\r\n        axios.post(this.url + '/api/mainpage/user_url_check/check/', {\r\n            username: this.username_url,\r\n          })\r\n          .then(res => {\r\n              this.username_url_check = res.data;\r\n\r\n          })\r\n\r\n    },\r\n\r\n    fun_after_token(){\r\n         this.check_sub();\r\n    },\r\n\r\n    user_check(){\r\n      axios.post(this.url + '/api/mainpage/user_check/get_username/', {\r\n\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.username = res.data;\r\n              this.fun_after_token();\r\n              if (this.username == this.username_url){\r\n                  this.access = true;\r\n              }\r\n          })\r\n\r\n    },\r\n\r\n    logout(){\r\n      localStorage.removeItem('access');\r\n      localStorage.removeItem('refresh');\r\n      this.access_token = null;\r\n      this.refresh_token = null;\r\n      this.$router.push('/')\r\n    },\r\n\r\n  },\r\n\r\n\r\n  props: {\r\n    image: {\r\n      type: Object,\r\n    },\r\n    coordinates: {\r\n      type: Object,\r\n    },\r\n    transitions: {\r\n      type: Object,\r\n    },\r\n    stencilCoordinates: {\r\n      type: Object,\r\n    },\r\n  },\r\n\r\n  computed: {\r\n    style() {\r\n      const { height, width, left, top } = this.stencilCoordinates;\r\n      const style = {\r\n        width: `${width}px`,\r\n        height: `${height}px`,\r\n        transform: `translate(${left}px, ${top}px)`,\r\n      };\r\n      if (this.transitions && this.transitions.enabled) {\r\n        style.transition = `${this.transitions.time}ms ${this.transitions.timingFunction}`;\r\n      }\r\n      return style;\r\n    },\r\n  },\r\n\r\n  created() {\r\n    this.user_url_check(); //существует ли пользователь\r\n    this.get_sum_sub(); // кол-во подписчиков и подписок у пользователя\r\n    this.access_token = localStorage.getItem('access');\r\n    this.refresh_token = localStorage.getItem('refresh');\r\n    if (this.access_token != null){\r\n      this.user_check()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n  .header {\r\n\theight: 54px;\r\n\tbackground: #333333;\r\n    border: 1px solid #444444;\r\n\t}\r\n  .flex {\r\n    display: flex;\r\n    /*justify-content: center;*/\r\n\t/*background: #9ba5b3; padding: 5px;*/\r\n  }\r\n  .itm_logo {\r\n    color: #eee;\r\n    margin: 6px;\r\n    /*flex-grow:1;*/\r\n    /*padding: 10px;*/\r\n    /*font-size: 2.5rem;*/\r\n  }\r\n  .itm_search{\r\n      margin: 10px;\r\n  }\r\n  .it_indent{\r\n      flex-basis: 10%\r\n  }\r\n  .itm_2{\r\n      flex-basis: 100%\r\n  }\r\n  .search{\r\n    width: 250px;\r\n    height: 30px;\r\n  }\r\n  .mainitem{\r\n      background: #252525;\r\n      min-height: 100vh;\r\n  }\r\n  .sideitem_1{\r\n      display: flex;\r\n      height: 200px;\r\n      flex-wrap: wrap;\r\n      align-content: center;\r\n  }\r\n  .itm_1_1{\r\n      flex-basis: 15%\r\n  }\r\n  .itm_1