AAA,KAAA,GAAA,GAAA,wCAAA,EAAA;AACA,UAAA,KAAA,EAAA,KAAA;AADA,SAAA,EAGA,IAHA,CAGA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,CAAA;AACA,SALA;AAMA;AACA,KAxBA;AA0BA,IAAA,UA1BA,wBA0BA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,yCAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,KADA;AAEA,QAAA,IAAA,EAAA,KAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,cAAA,GAAA,GAAA,CAAA,IAAA;;AACA,YAAA,MAAA,CAAA,cAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,CAAA;AACA;;AACA,YAAA,MAAA,CAAA,cAAA,IAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,CAAA;AACA;AACA,OAZA;AAcA,KAzCA;AA2CA,IAAA,QA3CA,sBA2CA;AAAA;;AACA,UAAA,KAAA,QAAA,IAAA,KAAA,cAAA,EAAA;AACA,aAAA,YAAA,GAAA,KAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,uBAAA,EAAA;AACA,UAAA,QAAA,EAAA,KAAA,QADA;AAEA,UAAA,QAAA,EAAA,KAAA,QAFA;AAGA,UAAA,KAAA,EAAA,KAAA;AAHA,SAAA,EAMA,IANA,CAMA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,CAAA;;AACA,UAAA,MAAA,CAAA,aAAA;AACA,SATA;AAYA,OAdA,MAeA;AACA,aAAA,YAAA,GAAA,IAAA;AACA;AACA,KA9DA;AA+DA,IAAA,aA/DA,2BA+DA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,4CAAA,EAAA;AACA,QAAA,QAAA,EAAA,KAAA,QADA;AAEA,QAAA,QAAA,EAAA,KAAA;AAFA,OAAA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA,CAEA,CANA;AAOA;AAvEA,GArBA;AA+FA,EAAA,OA/FA,qBA+FA,CAGA;AAlGA,CAAA","sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <br>\r\n\r\n    <p class=\"register_color\">Впервые тут?</p>\r\n    <div v-if=\"step == 1\">\r\n        <form @submit.prevent=\"check_data()\">\r\n            <p>\r\n              <input id=\"username\" v-model=\"username\" placeholder=\"Имя пользователя\" name=\"username\" required minlength=\"3\" pattern=\"[A-Za-z0-9]+\">\r\n            </p>\r\n            <p>\r\n                <input id=\"email\" type=\"email\" required v-model=\"email\" name=\"email\" placeholder=\"E-mail\">\r\n            </p>\r\n            <p class=\"register_color\">Дата рождения</p>\r\n            <p>\r\n                <input id=\"date\" type=\"date\" required v-model=\"birthday\" name=\"date\">\r\n            </p>\r\n            <p>\r\n                <button class=\"button_style\" type=\"submit\">Далее</button>\r\n            </p>\r\n        </form>\r\n        <p class=\"err\" v-if=\"check_data_res[1] == false\">Данная почта занята.</p>\r\n        <p class=\"err\" v-if=\"check_data_res[0] == false\">Имя пользователя занято.</p>\r\n    </div>\r\n\r\n    <div v-if=\"step == 2\">\r\n        <form @submit.prevent=\"check_code()\">\r\n            <p>\r\n                <input id=\"code\" v-model=\"code\" placeholder=\"Код\" name=\"code\" required>\r\n            </p>\r\n            <p>\r\n                <button class=\"button_style\" type=\"submit\">Далее</button>\r\n            </p>\r\n        </form>\r\n        <p class=\"register_color\" v-if=\"check_data_res[1] == true\">Код отправлен</p>\r\n\r\n    </div>\r\n    <p class=\"err\" v-if=\"check_code_res == 404\">Неправильный код</p>\r\n    <p class=\"err\" v-if=\"check_code_res == 502\">Сбой сервера. Попробуйте попытку позже.</p>\r\n    <div v-if=\"step == 3\">\r\n        <form @submit.prevent=\"register()\">\r\n\r\n            <p>\r\n              <input id=\"password\" placeholder=\"Пароль\" type=\"password\" v-model=\"password\" name=\"password\" required minlength=\"8\" pattern=\"(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{8,}\">\r\n            </p>\r\n            <p>\r\n              <input id=\"password_again\" placeholder=\"Подтверждение пароля\" type=\"password\" v-model=\"password_again\" name=\"password_again\" required minlength=\"8\" pattern=\"(?=.*\\d)(?=.*[a-z])(?=.*[A-Z]).{8,}\">\r\n            </p>\r\n            <p>\r\n                <button class=\"button_style\" type=\"submit\">Регистрация</button>\r\n            </p>\r\n        </form>\r\n        <p class=\"err\" v-if=\"this.err_password == true\">Пароли не совпадают.</p>\r\n    </div>\r\n\r\n    <p class=\"register_color\" v-if=\"step == 4\">Регистрация завершена.</p>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport axios from 'axios'\r\n\r\nimport vueSelect from 'vue-select'\r\n//import { TokenService } from './storage/TokenService.js'\r\n\r\nexport default {\r\n  name: 'registration',\r\n  components: {\r\n      vueSelect\r\n  },\r\n  data() {\r\n      return{\r\n          url: 'http://127.0.0.1:8000',\r\n          username: null,\r\n          password: null,\r\n          password_again: null,\r\n          email: null,\r\n          birthday: null,\r\n          code: null,\r\n          step: 1,\r\n          check_code_res: null,\r\n          check_data_res: Array,\r\n          err_password: false,\r\n\r\n      }\r\n  },\r\n   methods:{\r\n      check_data(){\r\n          this.check_code_res == null;\r\n          axios.post(this.url + '/api/startpage/verification/check_data/', {\r\n              email: this.email,\r\n              username: this.username,\r\n          })\r\n              .then(res => {\r\n                  this.check_data_res = res.data;\r\n                  this.check_code_res = null;\r\n                  this.send_email()\r\n              })\r\n\r\n      },\r\n\r\n       send_email(){\r\n          if (this.check_data_res[0] == true && this.check_data_res[1] == true){\r\n              axios.post(this.url + '/api/startpage/verification/send_code/', {\r\n              email: this.email,\r\n          })\r\n              .then(res => {\r\n                  this.step = 2;\r\n              })\r\n          }\r\n       },\r\n\r\n      check_code(){\r\n          axios.post(this.url + '/api/startpage/verification/check_code/', {\r\n              email: this.email,\r\n              code: this.code,\r\n          })\r\n          .then(res => {\r\n              this.check_code_res = res.data\r\n              if (this.check_code_res == 200){\r\n                  this.step = 3;\r\n              }\r\n              if (this.check_code_res == 404){\r\n                  this.step = 1;\r\n              }\r\n          })\r\n\r\n      },\r\n\r\n      register(){\r\n          if (this.password == this.password_again) {\r\n              this.err_password = false;\r\n              axios.post(this.url + '/api/startpage/users/', {\r\n                username: this.username,\r\n                password: this.password,\r\n                email: this.email,\r\n\r\n              })\r\n              .then(res => {\r\n                  this.step = 4;\r\n                  this.birthday_save()\r\n              });\r\n\r\n\r\n          }\r\n          else{\r\n               this.err_password = true\r\n          }\r\n      },\r\n      birthday_save(){\r\n          axios.post(this.url + '/api/startpage/user_profile/birthday_save/', {\r\n                username: this.username,\r\n                birthday: this.birthday,\r\n              })\r\n              .then(res => {\r\n\r\n              })\r\n      },\r\n\r\n   },\r\n   created() {\r\n\r\n\r\n   }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nbody {\r\n\tpadding: 5px;\r\n\tfont-size: 20px;\r\n}\r\n\r\ninput[type=\"number\"] {\r\n\twidth: 50px;\r\n\r\n}\r\n    .register_color{\r\n        color: #d9dbe0;\r\n    }\r\n\r\n    .date_format{\r\n        display: flex;\r\n    }\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/startpage"}]}