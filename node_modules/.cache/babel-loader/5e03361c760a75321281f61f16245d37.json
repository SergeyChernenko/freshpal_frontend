AA,IAAA,CAAA,EAAA;AACA,YAAA,CAAA,CAAA,cAAA;AACA,mBAAA,KAAA;AACA;AACA,SALA;;AAMA,YAAA,SAAA,GAAA,KAAA,OAAA,CAAA,OAAA,CAAA,QAAA,CAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA,EAAA,QAAA;AACA;AACA,KAlCA;AAoCA,IAAA,WApCA,yBAoCA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,mCAAA,EAAA,EAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAVA;AAWA;AAhDA,GAZA;AA+DA,EAAA,OA/DA,qBA+DA;AACA,SAAA,WAAA;AACA,GAjEA;AAkEA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,EAAA,gBAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,GAAA,CAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA;;AACA,UAAA,MAAA,CAAA,GAAA,CAAA,CAAA,MAAA,GAAA,CAAA,IAAA,MAAA,CAAA,GAAA,CAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,GAAA,GAAA,IAAA,IAAA,GAAA;AACA;;AACA,UAAA,MAAA,CAAA,GAAA,CAAA,CAAA,MAAA,GAAA,CAAA,IAAA,MAAA,CAAA,GAAA,CAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,GAAA,GAAA,OAAA,IAAA,GAAA;AACA;AACA;AAXA;AAlEA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"tag_style\">\r\n            <p class=\"p_tag\">Популярное за месяц</p>\r\n\r\n            <div style=\"margin-bottom: 7px\" v-for=\"hash in hashtag\">\r\n                <div style=\"display: flex\" v-on:click.middle=\"hashtag_page(hash.hashtag, true)\" v-on:click=\"hashtag_page(hash.hashtag, false)\" class=\"border_tag\">\r\n                    <span class=\"text_tag\">{{hash.hashtag | truncate(14, '...')}}</span>\r\n                    <div style=\"flex-grow: 1;\"></div>\r\n                    <span style=\"margin-right: 15px; color: #8e8e8e;\" class=\"count_tags\">{{ hash.count | number }}</span>\r\n                </div>\r\n            </div>\r\n            <div style=\"margin-bottom: 10px\" class=\"border_tag\" v-on:click.middle=\"hashtag_main(true)\" v-on:click=\"hashtag_main(false)\">\r\n                <span class=\"more_tag\">еще...</span>\r\n            </div>\r\n        </div>\r\n<!--        <div style=\"height: 16px\"></div>-->\r\n<!--        <div class=\"donate_style\">-->\r\n<!--            <p class=\"tags\">Подписчики</p>-->\r\n<!--        </div>-->\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n    name: \"RighMenu\",\r\n    components: {\r\n\r\n    },\r\n    data() {\r\n        return {\r\n            access_token: localStorage.getItem('access') || null,\r\n            new_mess: null,\r\n            hashtag: [],\r\n        }\r\n    },\r\n    methods: {\r\n\r\n        hashtag_page(tag,middle){\r\n            var url_page = '/hashtag/' + tag;\r\n            if (middle == false){\r\n                this.$router.push(url_page).catch(()=>{});\r\n            }\r\n            if (middle == true){\r\n                    document.body.onmousedown = function(e) {\r\n                        if(e.button == 1) {\r\n                            e.preventDefault();\r\n                            return false;\r\n                        }\r\n                    }\r\n                    let routeData = this.$router.resolve(url_page);\r\n                    window.open(routeData.href, '_blank');\r\n            }\r\n        },\r\n\r\n        hashtag_main(middle){\r\n            var url_page = '/hashtag';\r\n            if (middle == false){\r\n                this.$router.push(url_page).catch(()=>{});\r\n            }\r\n            if (middle == true){\r\n                    document.body.onmousedown = function(e) {\r\n                        if(e.button == 1) {\r\n                            e.preventDefault();\r\n                            return false;\r\n                        }\r\n                    }\r\n                    let routeData = this.$router.resolve(url_page);\r\n                    window.open(routeData.href, '_blank');\r\n            }\r\n        },\r\n\r\n        get_hashtag(){\r\n            axios.post(this.$hostname + '/api/home/home/get_hashtag_right/', {\r\n\r\n              },\r\n          {\r\n                headers: {\r\n                    'Authorization': 'Bearer ' + this.access_token,\r\n                }\r\n            })\r\n              .then(res => {\r\n                  this.hashtag = res.data;\r\n              })\r\n        },\r\n\r\n    },\r\n    created() {\r\n        this.get_hashtag()\r\n    },\r\n    filters: {\r\n        number: function(num) {\r\n            if (String(num).length < 5) {\r\n                return Math.floor(num);\r\n            }\r\n            if (String(num).length < 7 && String(num).length > 4) {\r\n                return Math.floor(num/1000) + 'т';\r\n            }\r\n            if (String(num).length < 9 && String(num).length > 6) {\r\n                return Math.floor(num/1000000) + 'м';\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components/Other"}]}