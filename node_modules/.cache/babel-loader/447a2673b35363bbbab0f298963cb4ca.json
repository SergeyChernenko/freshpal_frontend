EA,OAXA;AAYA,KAzBA;AA2BA,IAAA,SA3BA,uBA2BA;AAAA;;AACA,UAAA,KAAA,OAAA,IAAA,IAAA,IAAA,KAAA,OAAA,IAAA,EAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,KAAA,GAAA,GAAA,+BAAA,EAAA;AACA,UAAA,OAAA,EAAA,KAAA;AADA,SAAA,EAGA;AACA,UAAA,OAAA,EAAA;AACA,6BAAA,YAAA,KAAA;AADA;AADA,SAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA;AAEA,SAXA;AAYA,aAAA,OAAA,GAAA,IAAA;AACA;AACA,KA3CA;AA6CA,IAAA,UA7CA,wBA6CA;AAEA,UAAA,KAAA,QAAA,IAAA,KAAA,YAAA,EAAA;AACA,aAAA,MAAA,GAAA,IAAA;AACA;AAEA,KAnDA;AAoDA,IAAA,SApDA,uBAoDA;AACA,UAAA,QAAA,GAAA,MAAA,KAAA,YAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AAvDA,GApBA;AA8EA,EAAA,OA9EA,qBA8EA;AAAA;;AACA,IAAA,UAAA,CAAA;AAAA,aAAA,MAAA,CAAA,UAAA,EAAA;AAAA,KAAA,EAAA,GAAA,CAAA;AACA,SAAA,QAAA;AAEA;AAlFA,CAAA","sourcesContent":["\r\n<template>\r\n    <div>\r\n\r\n    <div v-if=\"access==true\" style=\"display: flex\">\r\n            <img class=\"img_input_m\" :src=\"url+'/media/profile/'+username+'/'+username+'.png'\">\r\n                <textarea\r\n                  rows=\"1\"\r\n                  class=\"input_mes\"\r\n                  placeholder=\"Что у вас нового?\"\r\n                  @input=\"resize\"\r\n                  v-model=\"message\"\r\n                ></textarea>\r\n            <button class=\"button_style but_input_m\" v-on:click=\"save_mess()\">Отправить</button>\r\n    </div>\r\n    <div style=\"height: 16px\"></div>\r\n    <div v-for=\"mess in all_data\">\r\n        <div class=\"border_st public_status\" v-on:click=\"status(mess[1][0])\">\r\n            <div style=\"display: flex\">\r\n                 <img class=\"img_p\" :src=\"url+'/media/profile/'+username_url+'/'+username_url+'.png'\">\r\n                <div class=\"username_p\">\r\n                    {{username_url}} · {{mess[0]}}\r\n                </div>\r\n\r\n            </div>\r\n            <div style=\"height: 8px\"></div>\r\n            <div class=\"just-line-break\">\r\n                {{mess[1][1]}}\r\n            </div>\r\n            <div style=\"height: 20px\"></div>\r\n\r\n        </div>\r\n        <div style=\"height: 20px\"></div>\r\n    </div>\r\n\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\n    import axios from \"axios\";\r\n\r\n    export default {\r\n        name: \"Publications\",\r\n\r\n    components: {\r\n\r\n\r\n    },\r\n    data(){\r\n      return{\r\n        url: this.$hostname,\r\n        access_token: localStorage.getItem('access') || null,\r\n        message: null,\r\n        username_url: this.$route.params.username,\r\n        access: false,\r\n        all_data: null,\r\n      }\r\n    },\r\n    props: {\r\n          username: String,\r\n    },\r\n    methods: {\r\n\r\n        status(id){\r\n            var url_page = '/' + this.username + '/status/' + id;\r\n            this.$router.push(url_page);\r\n        },\r\n\r\n        resize(event) {\r\n          event.target.style.height = \"auto\";\r\n          event.target.style.height = `${event.target.scrollHeight}px`;\r\n        },\r\n\r\n        get_mess(){\r\n            axios.post(this.url + '/api/mainpage/wall/get_mess/', {\r\n                username: this.username_url,\r\n              },\r\n          {\r\n                headers: {\r\n                    'Authorization': 'Bearer ' + this.access_token,\r\n                }\r\n            })\r\n              .then(res => {\r\n                this.all_data = res.data;\r\n\r\n              })\r\n        },\r\n\r\n        save_mess(){\r\n            if (this.message != null && this.message != ''){\r\n                axios.post(this.url + '/api/mainpage/wall/save_mess/', {\r\n                    message: this.message\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.get_mess()\r\n\r\n                  })\r\n                this.message = null;\r\n            }\r\n        },\r\n\r\n        user_check(){\r\n\r\n          if (this.username == this.username_url) {\r\n              this.access = true;\r\n          }\r\n\r\n        },\r\n        back_home(){\r\n                var url_page = '/' + this.username_url;\r\n                this.$router.push(url_page);\r\n            },\r\n\r\n    },\r\n    created() {\r\n        setTimeout(() => this.user_check(), 200);\r\n        this.get_mess()\r\n\r\n    },\r\n\r\n\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n\r\n\r\n\r\n</style>"],"sourceRoot":"src/components/userpage"}]}