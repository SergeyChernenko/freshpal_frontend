AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,oCAAA,EAAA;AACA,QAAA,OAAA,EAAA,OADA;AAEA,QAAA,MAAA,EAAA,MAFA;AAGA,QAAA,MAAA,EAAA,KAAA;AAHA,OAAA,EAKA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OALA,EAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,QAAA,QAAA,CAAA,MAAA;AACA,OAZA;AAaA,KA9FA;AAgGA,IAAA,SAhGA,uBAgGA;AACA,WAAA,IAAA,GAAA,IAAA,CADA,CAEA;AACA,KAnGA;AAqGA,IAAA,SArGA,qBAqGA,IArGA,EAqGA;AACA,UAAA,KAAA,SAAA,CAAA,SAAA,IAAA,IAAA,IAAA,IAAA,IAAA,CAAA,EAAA;AACA,YAAA,QAAA,GAAA,kBAAA,KAAA,SAAA,CAAA,SAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,OAHA,MAIA;AACA,YAAA,QAAA,GAAA,MAAA,KAAA,SAAA,CAAA,QAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AACA,KA9GA;AAgHA,IAAA,OAhHA,mBAgHA,QAhHA,EAgHA;AACA,UAAA,QAAA,GAAA,MAAA,QAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AAEA,KApHA;AAsHA,IAAA,kBAtHA,gCAsHA;AACA,UAAA,KAAA,YAAA,IAAA,KAAA,SAAA,CAAA,QAAA,EAAA;AACA,YAAA,QAAA,GAAA,MAAA,KAAA,SAAA,CAAA,QAAA,GAAA,UAAA,GAAA,KAAA,MAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AACA,KA3HA;AA6HA,IAAA,qBA7HA,mCA6HA;AAAA;;AACA,UAAA,eAAA,GAAA,MAAA,CAAA,QAAA,GAAA,EAAA;AACA,UAAA,GAAA,GAAA,IAAA,KAAA,EAAA;AACA,MAAA,GAAA,GAAA,eAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA,WAAA,MAAA,GAAA,GAAA,CAAA,CAAA,CAAA;AACA,WAAA,YAAA,GAAA,GAAA,CAAA,CAAA,CAAA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,oCAAA,EAAA;AACA,QAAA,MAAA,EAAA,KAAA;AADA,OAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,IAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,UAAA,GAAA,KAAA;AACA,SAFA,MAGA;AACA,UAAA,MAAA,CAAA,UAAA,GAAA,IAAA;AACA,UAAA,GAAA,CAAA,IAAA,CAAA,QAAA,GAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA,QAAA,CAAA,CAAA,MAAA,CAAA,oBAAA,CAAA;AACA,UAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;;AACA,UAAA,MAAA,CAAA,kBAAA;AACA;AAEA,OAnBA;AAqBA,KAxJA;AA2JA,IAAA,UA3JA,wBA2JA;AAAA;;AAAA;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,wCAAA,EAAA,EAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAVA;AAYA;AAxKA,GAhCA;AA2MA,EAAA,OA3MA,qBA2MA;AACA,SAAA,qBAAA;AACA,SAAA,WAAA;AACA,SAAA,UAAA;AAEA;AAhNA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <Header/>\r\n        <div class=\"background_main\">\r\n            <div class=\"center\">\r\n                <div style=\"height: 15px\"></div>\r\n                    <div style=\"display: flex;\">\r\n                        <div class=\"left_menu\">\r\n                            <LeftMenu/>\r\n                        </div>\r\n                        <div class=\"home_style\">\r\n                            <div v-if=\"check_publ == true\">\r\n                                <div style=\"margin-bottom: 10px;\">\r\n                                    <a v-if=\"data_mess.parent_id != null\" class=\"a_style\" v-on:click=\"back_home(1)\">Назад</a>\r\n                                </div>\r\n                                <div v-if=\"edit==false\" class=\"border_st\">\r\n                                    <div class=\"space-edit\">\r\n                                        <div style=\"display: flex\">\r\n                                             <a class=\"sub_link\" v-on:click=\"back_home()\"><img v-if=\"data_mess.username_id != null\" class=\"img_p\" :src=\"$hostname+'/media/profile/'+data_mess.username_id+'/'+data_mess.username_id+'.png'\"></a>\r\n                                            <div class=\"username_p\">\r\n                                                <a class=\"sub_link\" v-on:click=\"back_home()\">{{data_mess.username}}</a>\r\n                                            </div>\r\n\r\n\r\n                                        </div>\r\n                                        <div v-if=\"data_mess.access==true\" style=\"display: flex\">\r\n                                            <b-dropdown\r\n                                                 variant=\"link\"\r\n                                                 toggle-class=\"text-decoration-none\"\r\n                                                 no-caret>\r\n                                            <template #button-content>\r\n                                              <p><span class=\"select\">&hellip;</span></p>\r\n                                            </template>\r\n                                            <b-dropdown-item v-on:click=\"edit_publ()\"><p><span class=\"a_style\">Редактировать</span></p></b-dropdown-item>\r\n                                            <b-dropdown-item v-on:click=\"delete_publ()\"><p><span class=\"a_style_red\">Удалить</span></p></b-dropdown-item>\r\n                                          </b-dropdown>\r\n                                        </div>\r\n                                    </div>\r\n                                    <div v-if=\"data_mess.images.length!=0\">\r\n                                        <div class=\"mt-20 mb-20\">\r\n                                            <lightbox css=\"h-lg-400\" :items=\"data_mess.images\" :cells=\"3\"></lightbox>\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <div class=\"just-line-break\">\r\n                                        {{data_mess.message}}\r\n                                    </div>\r\n                                    <div style=\"height: 15px\"></div>\r\n                                    <div style=\"display: flex\">\r\n                                        {{data_mess.datetime}}\r\n                                        <div class=\"edir_purl_style\" v-if=\"data_mess.edit==true\">\r\n                                            <span class=\"edit_style\">изменено</span>\r\n                                        </div>\r\n                                    </div>\r\n                                    <hr>\r\n\r\n                                    <div style=\"display: flex\">\r\n                                         <Textarea\r\n                                             @message=\"SavePubl\"\r\n                                             :description=\"description\"\r\n                                             :username_id=\"username.username_id\"\r\n                                             :edit=\"false\"\r\n                                         />\r\n                                    </div>\r\n                                    <hr>\r\n                                    <div v-for=\"mess in all_comment\" class=\"public_status\" v-on:click=\"status(mess.username__username,mess.id)\">\r\n                                        <div class=\"space-edit\">\r\n                                            <div style=\"display: flex\">\r\n                                                <a class=\"sub_link\" v-on:click.stop=\"go_page(mess.username__username)\"><img v-if=\"mess.username != null\" class=\"img_p\" :src=\"$hostname+'/media/profile/'+mess.username+'/'+mess.username+'.png'\"></a>\r\n                                                <div class=\"username_p\">\r\n                                                    <a class=\"sub_link\" v-on:click.stop=\"go_page(mess.username__username)\">{{mess.username__username}}</a> · {{mess.datetime}}\r\n                                                </div>\r\n                                            </div>\r\n                                            <div class=\"username_p\" v-if=\"mess.edit==true\">\r\n                                                <span class=\"edit_style\">изм</span>\r\n                                            </div>\r\n                                        </div>\r\n                                        <div v-if=\"mess.images.length!=0\">\r\n                                            <div class=\"mt-20 mb-20\">\r\n                                                <div class=\"col-10\" v-on:click.stop=\"\">\r\n                                                    <lightbox css=\"h-lg-300\" :items=\"mess.images\" :cells=\"3\"></lightbox>\r\n                                                </div>\r\n                                            </div>\r\n                                        </div>\r\n                                        <div style=\"margin-top: 10px\">\r\n                                            <div class=\"just-line-break\">\r\n                                                {{mess.message}}\r\n                                            </div>\r\n                                             Комментировать\r\n                                        </div>\r\n\r\n                                        <hr>\r\n                                    </div>\r\n\r\n                                </div>\r\n\r\n                                <div v-if=\"edit==true\">\r\n                                    <Textarea\r\n                                     @save_edit=\"save_edit_publ\"\r\n                                     @cancel_edit=\"cancel_edit_mess\"\r\n                                     :description=\"description\"\r\n                                     :username_id=\"Number(data_mess.username_id)\"\r\n                                     :edit=\"true\"\r\n                                     :message_edit=\"data_mess.message\"\r\n                                     :images = \"data_mess.images\"/>\r\n                                </div>\r\n                            </div>\r\n                            <div v-if=\"check_publ == false\">\r\n                                <div style=\"height: 100px\"></div>\r\n                                <p class=\"text_style_2\">Публикации не существует</p>\r\n                            </div>\r\n                        </div>\r\n                        <div class=\"right_menu\">\r\n                            <RighMenu/>\r\n                        </div>\r\n                    </div>\r\n                <div v-observe-visibility=\"handleScrolledToBottom\"></div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import Vue from 'vue'\r\n    import TextareaAutosize from 'vue-textarea-autosize'\r\n    import axios from \"axios\";\r\n    import VueObserveVisibility from 'vue-observe-visibility'\r\n    import moment from 'moment'\r\n    Vue.use(VueObserveVisibility)\r\n    Vue.use(TextareaAutosize)\r\n    moment.locale('ru')\r\n    export default {\r\n        name: \"Status\",\r\n        components: {\r\n        },\r\n        data() {\r\n            return {\r\n                access_token: localStorage.getItem('access') || null,\r\n                id_url: null,\r\n                username_url: null,\r\n                data_mess: {\r\n                    username__username: null,\r\n                    message: null,\r\n                    datetime: null,\r\n                    edit: null,\r\n                    access: null,\r\n                    parent_id: null,\r\n                    images: []\r\n                },\r\n                edit: false,\r\n                message: null,\r\n                username: {\r\n                    username: null,\r\n                    username_id: null,\r\n                },\r\n                comment: null,\r\n                all_comment: [],\r\n                last_datetime: null,\r\n                stop_get_comment: 8,\r\n                description: 'Добавьте комментарий...',\r\n                check_publ: Boolean,\r\n            }\r\n        },\r\n        methods: {\r\n\r\n            handleScrolledToBottom(isVisible){\r\n                if (!isVisible)  { return }\r\n                if (this.all_comment == null) { return }\r\n                if (this.last_datetime == null) { return }\r\n                this.get_comment()\r\n            },\r\n\r\n            status(username,id){\r\n                var url_page = '/' + username + '/status/' + id;\r\n                this.$router.push(url_page);\r\n            },\r\n\r\n\r\n\r\n            get_comment(){\r\n                if (this.stop_get_comment != 0){\r\n                    axios.post(this.$hostname + '/api/mainpage/publ/get_comment/', {\r\n                        parent_id: this.id_url,\r\n                        datetime_request: this.last_datetime\r\n                      },\r\n                  {\r\n                        headers: {\r\n                            'Authorization': 'Bearer ' + this.access_token,\r\n                        }\r\n                    })\r\n                      .then(res => {\r\n                          if (res.data != false){\r\n                              this.stop_get_comment = res.data.length\r\n                              this.last_datetime = res.data[res.data.length-1].datetime\r\n                              for (var i = 0; i < res.data.length; i++){\r\n                                    res.data[i].datetime = moment(res.data[i].datetime).format('DD MMMM YY')\r\n                                }\r\n                              this.all_comment.push(...res.data);\r\n                          }\r\n\r\n                      })\r\n                }\r\n            },\r\n\r\n            SavePubl(comment,images){\r\n                if (comment != '' || images.length != 0){\r\n                    this.stop_get_comment = 10\r\n                    this.last_datetime = null\r\n                    this.all_comment = []\r\n                    axios.post(this.$hostname + '/api/mainpage/publ/save_comment/', {\r\n                        comment: comment,\r\n                        images: images,\r\n                        parent_id: this.id_url,\r\n                      },\r\n                  {\r\n                        headers: {\r\n                            'Authorization': 'Bearer ' + this.access_token,\r\n                        }\r\n                    })\r\n                      .then(res => {\r\n                          this.get_comment()\r\n                      })\r\n                }\r\n            },\r\n\r\n            delete_publ(){\r\n                axios.post(this.$hostname+ '/api/mainpage/publ/delete_mess/', {\r\n                    id_url: this.id_url,\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.back_home(1)\r\n                  })\r\n            },\r\n\r\n            cancel_edit_mess(){\r\n                this.edit = false;\r\n            },\r\n\r\n            save_edit_publ(message,images){\r\n                axios.post(this.$hostname + '/api/mainpage/publ/save_edit_publ/', {\r\n                    message: message,\r\n                    images: images,\r\n                    id_url: this.id_url,\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                    location.reload()\r\n                  })\r\n            },\r\n\r\n            edit_publ(){\r\n              this.edit = true;\r\n              // this.message = this.data_mess.message;\r\n            },\r\n\r\n            back_home(flag){\r\n                if (this.data_mess.parent_id != null && flag == 1){\r\n                    var url_page = '/back/status/' + this.data_mess.parent_id;\r\n                    this.$router.push(url_page);\r\n                }\r\n                else{\r\n                    var url_page = '/' + this.data_mess.username;\r\n                    this.$router.push(url_page);\r\n                }\r\n            },\r\n\r\n            go_page(username){\r\n                var url_page = '/' + username;\r\n                this.$router.push(url_page);\r\n\r\n            },\r\n\r\n            check_url_username(){\r\n                if (this.username_url != this.data_mess.username){\r\n                    var url_page = '/' + this.data_mess.username + '/status/' + this.id_url;\r\n                    this.$router.push(url_page);\r\n                }\r\n            },\r\n\r\n            check_id_and_username(){\r\n                var currentLocation = window.location + '';\r\n                let arr = new Array();\r\n                arr = currentLocation.split('/')\r\n                this.id_url = arr[6]\r\n                this.username_url = arr[4]\r\n                axios.post(this.$hostname + '/api/mainpage/publ/get_publ_by_id/', {\r\n                    id_url: this.id_url,\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                    if (res.data == false){\r\n                        this.check_publ = false\r\n                    }\r\n                    else{\r\n                        this.check_publ = true\r\n                        res.data.datetime = moment(res.data.datetime).format('hh:mm DD MMMM YYYY')\r\n                        this.data_mess = res.data;\r\n                        this.check_url_username()\r\n                    }\r\n\r\n                  })\r\n\r\n            },\r\n\r\n\r\n            user_check(){ //валидация пользователя из url\r\n              axios.post(this.$hostname + '/api/mainpage/user_check/get_username/', {\r\n\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.username = res.data;\r\n                  })\r\n\r\n            },\r\n\r\n        },\r\n        created() {\r\n            this.check_id_and_username()\r\n            this.get_comment()\r\n            this.user_check();\r\n\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n\r\n\r\n\r\n\r\n</style>"],"sourceRoot":"src/components/userpage"}]}