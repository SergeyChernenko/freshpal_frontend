template>\r\n    <header class=\"header_style\">\r\n      <div class=\"center\">\r\n          <div class=\"flex\">\r\n\r\n              <div class=\"itm_logo\"><img width=\"120\" height=\"40\" :src=\"url+'/media/logo.svg'\"></div>\r\n              <div class=\"itm_2\"></div>\r\n              <div class=\"itm_search\"><input class=\"search\" type=\"search\" name=\"search\" placeholder=\"Поиск\"></div>\r\n              <div class=\"itm_2\"></div>\r\n              <a :href=\"home\"><img width=\"27\" height=\"27\" class=\"home_header\" :src=\"url+'/media/home.svg'\"></a>\r\n\r\n\r\n              <div>\r\n                 <b-dropdown\r\n                         variant=\"link\"\r\n                         toggle-class=\"text-decoration-none\"\r\n                         no-caret\r\n\r\n                 >\r\n                    <template #button-content>\r\n                      <img v-if=\"username != null\" class=\"img_header\" :src=\"url+'/media/profile/'+username+'/'+username+'.png'\"><span class=\"sr-only\"></span>\r\n                    </template>\r\n                    <b-dropdown-item v-on:click=\"back_home()\">Профиль</b-dropdown-item>\r\n                    <b-dropdown-divider></b-dropdown-divider>\r\n                    <b-dropdown-item v-on:click=\"logout()\">Выйти</b-dropdown-item>\r\n\r\n                  </b-dropdown>\r\n\r\n              </div>\r\n\r\n\r\n          </div>\r\n      </div>\r\n\r\n\t</header>\r\n</template>\r\n\r\n<script>\r\n    import {CircleStencil, Cropper, Preview} from \"vue-advanced-cropper\";\r\n    import ImageUploader from \"vue-image-upload-resize\";\r\n    import axios from \"axios\";\r\n\r\n    export default {\r\n        name: \"Header\",\r\n        components: {\r\n\r\n\r\n        },\r\n        data(){\r\n          return{\r\n              home: this.$localhost + '/home',\r\n              url: this.$hostname,\r\n              username: null,\r\n              access_token: localStorage.getItem('access') || null,\r\n          }\r\n        },\r\n        methods: {\r\n            user_check(){\r\n              axios.post(this.url + '/api/mainpage/user_check/get_username/', {\r\n\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.username = res.data;\r\n                  })\r\n\r\n            },\r\n\r\n            back_home(){\r\n                var url_page = '/' + this.username;\r\n                this.$router.push(url_page);\r\n\r\n            },\r\n            logout(){\r\n              localStorage.removeItem('access');\r\n              localStorage.removeItem('refresh');\r\n              this.access_token = null;\r\n              this.refresh_token = null;\r\n              this.$router.push('/')\r\n            },\r\n\r\n            },\r\n        created() {\r\n            this.user_check()\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n    .header_style {\r\n        height: 54px;\r\n        background: #222222;\r\n        border: 1px solid #393939;\r\n        /*position: fixed;*/\r\n        /*width: 100%;*/\r\n\t}\r\n    .center {\r\n        width: 1000px; /* Ширина элемента в пикселах */\r\n        margin: auto; /* Выравниваем по центру */\r\n        min-height: 100vh;\r\n   }\r\n    .flex {\r\n        display: flex;\r\n        /*justify-content: center;*/\r\n        /*background: #9ba5b3; padding: 5px;*/\r\n  }\r\n    .itm_logo {\r\n        color: #eee;\r\n        margin: 5px;\r\n        /*flex-grow:1;*/\r\n        /*padding: 10px;*/\r\n        /*font-size: 2.5rem;*/\r\n  }\r\n  .itm_search{\r\n          margin: 10px;\r\n          border: 2px solid #393939;\r\n          border-radius: 12px;\r\n  }\r\n  .itm_2{\r\n      flex-basis: 100%\r\n  }\r\n  .home_header{\r\n        margin: 15px 7px 0px 15px;\r\n    }\r\n\r\n\r\n</style>"],"sourceRoot":"src/components/user