      </div>\r\n\r\n                <div class=\"home_style\">\r\n                    <div v-if=\"username_url_data.check == true\">\r\n                        <div class=\"sideitem_1\">\r\n                            <image-uploader\r\n                                :maxWidth=\"700\"\r\n                                :maxHeight=\"700\"\r\n                                :quality=\"1\"\r\n                                :className=\"['fileInput', { 'fileinput--loaded': hasImage }]\"\r\n                                doNotResize=\"['gif', 'svg']\"\r\n                                outputFormat=\"string\"\r\n                                ref=\"avatarGet\"\r\n                                :preview=false\r\n                                @input=\"setImage\"\r\n                            >\r\n                            </image-uploader>\r\n\r\n\r\n\r\n                            <div style=\"display: flex\">\r\n                                <div class=\"avatar_click\">\r\n                                    <img v-if=\"username_url_data.id != null\" style=\"border-radius: 50%\" :src=\"$hostname+'/media/profile/'+username_url_data.id+'/'+username_url_data.id+'.png'\">\r\n                                    <label v-if=\"username_url_data.access==true\" for=\"fileInput\"><div id='invisible'></div></label>\r\n                                </div>\r\n                                <div>\r\n                                    <div style=\"display: flex\">\r\n                                        <p class=\"username_style margin_img\"> {{ username_url }}</p>\r\n                                        <p v-if=\"username_url_data.positive == true\" class=\"margin_img\" align=\"left\"><span class=\"level_page_style_true\">{{username_url_data.level}}</span></p>\r\n                                        <p v-if=\"username_url_data.positive == false\" class=\"margin_img\" align=\"left\"><span class=\"level_page_style_false\">{{username_url_data.level}}</span></p>\r\n                                        <div style=\"flex-grow: 1;\"></div>\r\n                                        <div v-if=\"username_url_data.access == false\">\r\n                                            <button style=\"width: 120px\" v-if=\"check_sub_res == false\" class=\"button_style margin_but_sub\" v-on:click=\"subscribe()\">Подписаться</button>\r\n                                            <button style=\"width: 120px\" v-if=\"check_sub_res == true\" class=\"button_style margin_but_sub\" v-on:click=\"unsubscribe()\">Отписаться</button>\r\n                                        </div>\r\n                                    </div>\r\n\r\n                                    <div style=\"display: flex\">\r\n                                        <p class=\"text_style margin_sub\">0 публикации </p>\r\n                                        <p class=\"text_style margin_sub\"><a class=\"sub_link\" v-on:click=\"followers_show()\">{{sum_sub.subscribe}} подписчиков</a></p>\r\n                                        <p class=\"text_style margin_sub\"><a class=\"sub_link\" v-on:click=\"following_show()\">{{sum_sub.subscriptions}} подписок</a></p>\r\n                                    </div>\r\n                                </div>\r\n                            </div>\r\n\r\n\r\n                        </div>\r\n                        <hr>\r\n                        <Publications\r\n                        @update_rating=\"update_rating\"\r\n                        :username_url_id=\"username_url_data.id\"\r\n                        />\r\n                    </div>\r\n                    <div v-if=\"username_url_data.check == false\">\r\n                        <div style=\"height: 100px\"></div>\r\n                        <p class=\"text_style_2\">Пользователя {{ this.username_url }} не существует</p>\r\n                    </div>\r\n                </div>\r\n\r\n\r\n                <div class=\"right_menu\">\r\n                    <RighMenu/>\r\n                </div>\r\n            </div>\r\n            <modal name=\"followers\"\r\n               :width=\"420\"\r\n               :height=\"480\"\r\n               :pivot-y=\"0.5\"\r\n               :adaptive=\"false\"\r\n               :scrollable=\"true\"\r\n               styles=\"border-radius: 17px; background: #191919; box-shadow: 0 0 1px 1px #393939\"\r\n               transition=\"nice-modal-fade\">\r\n                <p class=\"text_style_2\">Подписчики</p>\r\n                <hr>\r\n                <div v-for=\"follower in list_followers\">\r\n                    <div class=\"list_modal_style\">\r\n                        <div class=\"body_sub\">\r\n                           <img class=\"img_sub\" :src=\"$hostname+'/media/profile/'+follower.subscriber__id+'/'+follower.subscriber__id+'.png'\">\r\n                           <div style=\"width: 10px\"></div>\r\n                           <p class=\"text_style\"><a class=\"sub_link\" :href=\"follower.subscriber__username\">{{follower.subscriber__username}}</a></p>\r\n                           <div style=\"width: 200px\"></div>\r\n                            <button class=\"button_style\" v-on:click=\"subscribe()\">Подписаться</button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </modal>\r\n\r\n            <modal name=\"following\"\r\n               :width=\"420\"\r\n               :height=\"480\"\r\n               :pivot-y=\"0.5\"\r\n               :adaptive=\"true\"\r\n               :scrollable=\"true\"\r\n               styles=\"border-radius: 17px; background: #191919; box-shadow: 0 0 1px 1px #393939\"\r\n               transition=\"nice-modal-fade\">\r\n               <p class=\"text_style_2\">Подписки</p>\r\n                <hr>\r\n                <div v-for=\"following in list_following\">\r\n                    <div class=\"list_modal_style\">\r\n                        <div class=\"body_sub\">\r\n                           <img class=\"img_sub\" :src=\"$hostname+'/media/profile/'+following.username__id+'/'+following.username__id+'.png'\">\r\n\r\n                           <p class=\"text_style\"><a class=\"sub_link\" :href=\"following.username__username\">{{following.username__username}}</a></p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </modal>\r\n\r\n\r\n\r\n             <modal name=\"crop_avatar\"\r\n               :width=\"470\"\r\n               height=\"auto\"\r\n               :pivot-y=\"0.5\"\r\n               :adaptive=\"true\"\r\n               :scrollable=\"true\"\r\n\r\n               styles=\"border-radius: 17px; background: #191919; box-shadow: 0 0 1px 1px #393939\"\r\n               transition=\"nice-modal-fade\">\r\n               <div style=\"display: flex\">\r\n                 <cropper\r\n                    ref=\"cropper\"\r\n                    class=\"avatat-result__cropper\"\r\n                    :src=\"url_avatar\"\r\n                    :debounce=\"false\"\r\n                    :stencil-props=\"{\r\n                        aspectRatio: 1,\r\n\r\n                    }\"\r\n                    :stencil-component=\"$options.components.CircleStencil\"\r\n                    @change=\"onChange\"\r\n                />\r\n                <div class=\"avatat-result__previews\">\r\n                    <preview style=\"\" class=\"avatat-result__preview\" :image=\"result.image\" :coordinates=\"result.coordinates\" />\r\n                    <div style=\"height: 15px\"></div>\r\n                    <label for=\"fileInput\"><p><a class=\"sub_link\">Изменить</a></p></label>\r\n                </div>\r\n\r\n                </div>\r\n\r\n                <div class=\"button_avatar\">\r\n                    <button class=\"button_style\" v-on:click=\"onCrop\">Сохранить</button>\r\n                    <div style=\"width: 25px\"></div>\r\n                    <button class=\"button_style\" v-on:click=\"crop_avatar_hide\">Выйти</button>\r\n                </div>\r\n\r\n            </modal>\r\n        </div>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport axios from 'axios';\r\nimport VModal from 'vue-js-modal';\r\nimport  Vue from  'vue';\r\nimport { CircleStencil, Cropper, Preview  } from 'vue-advanced-cropper';\r\nimport 'vue-advanced-cropper/dist/style.css';\r\nimport ImageUploader from 'vue-image-upload-resize'\r\n\r\n\r\nVue.use(ImageUploader);\r\nVue.use(VModal);\r\n// Vue.forceUpdate();\r\n\r\nexport default {\r\n  name: 'startpage',\r\n  components: {\r\n      Cropper, CircleStencil, Preview,\r\n      ImageUploader,\r\n  },\r\n\r\n  data(){\r\n      return{\r\n        access_token: localStorage.getItem('access') || null,\r\n        username_url: this.$route.params.username,\r\n        username: null,\r\n        access: null,\r\n        username_url_data: {\r\n            check: null,\r\n            id: null,\r\n            access: null,\r\n            level: null,\r\n            positive: null,\r\n        },\r\n        sum_sub: {\r\n            subscribe: null,\r\n            subscriptions: null,\r\n        },\r\n        check_sub_res: null,\r\n        selected_avatar: null,\r\n        url_avatar: null,\r\n        list_followers: Array,\r\n        list_following: Array,\r\n        result: {\r\n\t\t\t\tcoordinates: null,\r\n\t\t\t\timage: null,\r\n\t\t\t},\r\n        hasImage: false,\r\n        res_avatar: null,\r\n        test_img: null,\r\n      }\r\n  },\r\n\r\n  methods: {\r\n\r\n    update_rating(level, positive){\r\n        this.username_url_data.level = level\r\n        this.username_url_data.positive = positive\r\n    },\r\n\r\n    setImage(output) {\r\n      this.hasImage = true;\r\n      this.url_avatar = output;\r\n      this.crop_avatar_show()\r\n    },\r\n\r\n    onChange({ coordinates, image }) {\r\n\r\n        this.result = {\r\n            coordinates,\r\n            image,\r\n        };\r\n    },\r\n\r\n    onCrop() {\r\n        const result = this.$refs.cropper.getResult().canvas.toDataURL();\r\n        this.test_img = result\r\n        axios.post(this.$hostname + '/api/mainpage/avatar/set/', {\r\n            avatar: result\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.res_avatar = res.data;\r\n              location.reload();\r\n          });\r\n\r\n    },\r\n\r\n    // avatar_selected(event){\r\n    //     this.url_avatar = null;\r\n    //     this.selectedAvatar = event.target.files[0];\r\n    //     this.url_avatar = URL.createObjectURL(this.selectedAvatar);\r\n    //     console.log(this.url_avatar)\r\n    //     this.crop_avatar_show()\r\n    //\r\n    // },\r\n    crop_avatar_show(){\r\n\r\n        this.$modal.show('crop_avatar');\r\n    },\r\n    crop_avatar_hide(){\r\n        location.reload();\r\n        this.$modal.hide('crop_avatar');\r\n    },\r\n\r\n    followers_show(){\r\n        axios.post(this.$hostname + '/api/mainpage/subscribe/list_followers/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.list_followers = res.data;\r\n          });\r\n        this.$modal.show('followers');\r\n    },\r\n    // followers_hide () {\r\n    //     this.$modal.hide('followers');\r\n    // },\r\n    following_show(){\r\n        axios.post(this.$hostname + '/api/mainpage/subscribe/list_following/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n            headers: {\r\n                'Authorization': 'Bearer ' + this.access_token,\r\n            }\r\n        })\r\n          .then(res => {\r\n              this.list_following = res.data;\r\n          })\r\n        this.$modal.show('following');\r\n    },\r\n\r\n    check_sub(){\r\n        axios.post(this.$hostname + '/api/mainpage/subscribe/check_sub/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data;\r\n\r\n          })\r\n    },\r\n\r\n    get_sum_sub(){\r\n        axios.post(this.$hostname + '/api/mainpage/subscribe/sum/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.sum_sub = res.data;\r\n          })\r\n    },\r\n\r\n    subscribe(){\r\n        axios.post(this.$hostname + '/api/mainpage/subscribe/plus/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data\r\n              this.get_sum_sub()\r\n          })\r\n    },\r\n\r\n    unsubscribe(){\r\n        axios.post(this.$hostname + '/api/mainpage/subscribe/minus/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data;\r\n              this.get_sum_sub()\r\n          })\r\n    },\r\n\r\n    user_url_check(){\r\n        axios.post(this.$hostname + '/api/mainpage/user_check/check/', {\r\n            username: this.username_url,\r\n        },\r\n        {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n          })\r\n          .then(res => {\r\n              this.username_url_data = res.data;\r\n\r\n          })\r\n    },\r\n\r\n    timeout(){\r\n        if ( this.username_url_data.check != null ) {\r\n            if (