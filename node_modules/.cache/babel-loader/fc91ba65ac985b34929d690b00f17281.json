CA,UAAA,QAAA,GAAA,MAAA,KAAA,YAAA,GAAA,UAAA,GAAA,EAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA,KA1EA;AA4EA,IAAA,QA5EA,sBA4EA;AAAA;;AACA,UAAA,KAAA,aAAA,IAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,8BAAA,EAAA;AACA,UAAA,QAAA,EAAA,KAAA,YADA;AAEA,UAAA,gBAAA,EAAA,KAAA;AAFA,SAAA,EAIA;AACA,UAAA,OAAA,EAAA;AACA,6BAAA,YAAA,KAAA;AADA;AADA,SAJA,EASA,IATA,CASA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,IAAA,KAAA,EAAA;AAAA;;AACA,YAAA,MAAA,CAAA,aAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,YAAA,MAAA,CAAA,aAAA,GAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA,GAAA,CAAA,EAAA,QAAA;;AACA,iBAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,QAAA,GAAA,MAAA,CAAA,GAAA,CAAA,IAAA,CAAA,CAAA,EAAA,QAAA,CAAA,CAAA,MAAA,CAAA,YAAA,CAAA;AACA;;AACA,+BAAA,MAAA,CAAA,QAAA,EAAA,IAAA,2CAAA,GAAA,CAAA,IAAA;AACA;AAEA,SAnBA;AAoBA;AACA,KAnGA;AAqGA,IAAA,QArGA,oBAqGA,OArGA,EAqGA,MArGA,EAqGA;AAAA;;AACA,UAAA,OAAA,IAAA,EAAA,IAAA,MAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,aAAA,aAAA,GAAA,EAAA;AACA,aAAA,aAAA,GAAA,IAAA;AACA,aAAA,QAAA,GAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,+BAAA,EAAA;AACA,UAAA,OAAA,EAAA,OADA;AAEA,UAAA,MAAA,EAAA;AAFA,SAAA,EAIA;AACA,UAAA,OAAA,EAAA;AACA,6BAAA,YAAA,KAAA;AADA;AADA,SAJA,EASA,IATA,CASA,UAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA;AAEA,SAZA;AAcA;AACA,KAzHA;AA4HA,IAAA,UA5HA,wBA4HA;AAAA;;AACA,MAAA,KAAA,CAAA,IAAA,CAAA,KAAA,SAAA,GAAA,wCAAA,EAAA,EAAA,EAGA;AACA,QAAA,OAAA,EAAA;AACA,2BAAA,YAAA,KAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA;;AACA,YAAA,MAAA,CAAA,QAAA,IAAA,MAAA,CAAA,YAAA,EAAA;AACA,UAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA;AACA,OAbA;AAcA,KA3IA;AA4IA,IAAA,SA5IA,uBA4IA;AACA,UAAA,QAAA,GAAA,MAAA,KAAA,YAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA,EAAA,KAAA,CAAA,YAAA,CAAA,CAAA;AACA;AA/IA,GA7BA;AA+KA,EAAA,OA/KA,qBA+KA;AACA,SAAA,UAAA;AACA,SAAA,QAAA;AAEA,GAnLA;AAqLA,EAAA,OAAA,EAAA;AACA,IAAA,MAAA,EAAA,gBAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,GAAA,CAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA;;AACA,UAAA,MAAA,CAAA,GAAA,CAAA,CAAA,MAAA,GAAA,CAAA,IAAA,MAAA,CAAA,GAAA,CAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,GAAA,GAAA,IAAA,IAAA,GAAA;AACA;;AACA,UAAA,MAAA,CAAA,GAAA,CAAA,CAAA,MAAA,GAAA,CAAA,IAAA,MAAA,CAAA,GAAA,CAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,GAAA,GAAA,OAAA,IAAA,GAAA;AACA;AACA;AAXA;AArLA,CAAA","sourcesContent":["\r\n<template>\r\n    <div>\r\n        <div v-if=\"access==true\">\r\n\r\n                <Textarea\r\n                    @message=\"SavePubl\"\r\n                    :description=\"description\"\r\n                    :username_id=\"username_url_id\"\r\n                    :edit=\"false\"\r\n                />\r\n            <hr>\r\n        </div>\r\n\r\n        <div style=\"margin-top: 16px\" v-for=\"(mess,index) in all_data\">\r\n            <div style=\"margin-bottom: 20px\" class=\"border_st public_status\" v-on:click=\"status(mess.id)\">\r\n                <div class=\"space-edit\">\r\n                    <div style=\"display: flex\">\r\n                        <a class=\"sub_link\"><img v-if=\"username_url_id != null\" class=\"img_p\" :src=\"$hostname+'/media/profile/'+username_url_id+'/'+username_url_id+'.png'\"></a>\r\n                        <div class=\"username_p\">\r\n                            <a>{{username_url}}</a> · {{mess.datetime}}\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"username_p\" v-if=\"mess.edit==true\">\r\n                        <span class=\"edit_style\">изменено</span>\r\n                    </div>\r\n                </div>\r\n                <div v-if=\"mess.images.length!=0\">\r\n                    <div class=\"mt-10 mb-10\" v-on:click.stop.prevent=\"\">\r\n                        <lightbox css=\"h-lg-400\" :items=\"mess.images\" :cells=\"3\"></lightbox>\r\n                    </div>\r\n                </div>\r\n                <div style=\"margin-top: 10px\" class=\"just-line-break\">\r\n                    {{mess.message}}\r\n                </div>\r\n\r\n                <div class=\"flex_like_space\">\r\n                    <div style=\"width: 25px; margin-left: 5px\">\r\n                        <p v-if=\"mess.positive == true\"><span class=\"level_publ_style_true\">{{mess.level}}</span></p>\r\n                        <p v-if=\"mess.positive == false\"><span class=\"level_publ_style_false\">{{mess.level}}</span></p>\r\n                    </div>\r\n                    <div v-on:click.stop.prevent=\"like(mess.id,index)\" class=\"like_mainpage\" style=\"margin-left: 25px\">\r\n                        <div v-if=\"mess.sum_like > 0\" style=\"padding-right: 5px\">{{ mess.sum_like | number }}</div>\r\n                        <img v-if=\"mess.check_like == true\" style=\"margin-top: 1px\" width=\"18\" height=\"18\" :src=\"$hostname+'/media/like_done.svg'\">\r\n                        <img v-if=\"mess.check_like == null\" style=\"margin-top: 1px\" width=\"18\" height=\"18\" :src=\"$hostname+'/media/like.svg'\">\r\n                    </div>\r\n                    <div v-on:click.stop.prevent=\"dislike(mess.id,index)\" class=\"like_mainpage\" style=\"margin-left: 35px\">\r\n                        <div v-if=\"mess.sum_dislike > 0\" style=\"padding-right: 5px\">{{ mess.sum_dislike | number }}</div>\r\n                        <img v-if=\"mess.check_dislike == true\" style=\"margin-top: 1px\" width=\"18\" height=\"18\" :src=\"$hostname+'/media/dislike_done.svg'\">\r\n                        <img v-if=\"mess.check_dislike == null\" style=\"margin-top: 1px\" width=\"18\" height=\"18\" :src=\"$hostname+'/media/dislike.svg'\">\r\n                    </div>\r\n                    <div style=\"display: flex; margin-left: 35px\">\r\n                        <div style=\"padding-right: 5px\">{{ 0 | number }}</div>\r\n                        <img style=\"margin-top: 1px\" width=\"18\" height=\"18\" :src=\"$hostname+'/media/comment.svg'\">\r\n                    </div>\r\n                </div>\r\n\r\n            </div>\r\n        </div>\r\n\r\n        <div v-observe-visibility=\"handleScrolledToBottom\"></div>\r\n        <div style=\"height: 5px\"></div>\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\n    import Vue from 'vue'\r\n    import TextareaAutosize from 'vue-textarea-autosize'\r\n    import axios from \"axios\";\r\n    import moment from \"moment\";\r\n    import VueObserveVisibility from \"vue-observe-visibility\";\r\n    import '@morioh/v-lightbox/dist/lightbox.css';\r\n    import Lightbox from '@morioh/v-lightbox'\r\n\r\n    Vue.use(Lightbox);\r\n    Vue.use(TextareaAutosize)\r\n    Vue.use(VueObserveVisibility)\r\n\r\n\r\n    export default {\r\n        name: \"Publications\",\r\n\r\n    components: {\r\n    },\r\n\r\n    data(){\r\n      return{\r\n        access_token: localStorage.getItem('access') || null,\r\n        username_url: this.$route.params.username,\r\n        access: false,\r\n        all_data: [],\r\n        last_datetime: null,\r\n        stop_get_pudl: 10,\r\n        username: null,\r\n        description: 'Что у вас нового?',\r\n        rating_result:{\r\n            like: null,\r\n            dislike: null,\r\n            level_p: null,\r\n            positive_p: null,\r\n            level_user: null,\r\n            positive_user: null,\r\n        }\r\n      }\r\n    },\r\n    props: {\r\n          username_url_id: Number,\r\n    },\r\n    methods: {\r\n\r\n        view_update_rating(index){\r\n            this.$emit('update_rating', this.rating_result.level_user, this.rating_result.positive_user);\r\n            this.all_data[index].level = this.rating_result.level_p\r\n            this.all_data[index].positive = this.rating_result.positive_p\r\n            if (this.rating_result.like == true){\r\n                this.all_data[index].check_like = true\r\n                this.all_data[index].sum_like = this.all_data[index].sum_like + 1\r\n            }\r\n            if (this.rating_result.like == false){\r\n                this.all_data[index].check_like = null\r\n                this.all_data[index].sum_like = this.all_data[index].sum_like - 1\r\n            }\r\n            if (this.rating_result.dislike == true){\r\n                this.all_data[index].check_dislike = true\r\n                this.all_data[index].sum_dislike = this.all_data[index].sum_dislike + 1\r\n            }\r\n            if (this.rating_result.dislike == false){\r\n                this.all_data[index].check_dislike = null\r\n                this.all_data[index].sum_dislike = this.all_data[index].sum_dislike - 1\r\n            }\r\n        },\r\n\r\n        like(id, index){\r\n            if (this.username != this.username_url){\r\n                axios.post(this.$hostname + '/api/mainpage/rating/like/', {\r\n                    id: id\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.rating_result = res.data;\r\n                      this.view_update_rating(index)\r\n                  })\r\n            }\r\n        },\r\n\r\n        dislike(id, index){\r\n            if (this.username != this.username_url){\r\n                axios.post(this.$hostname + '/api/mainpage/rating/dislike/', {\r\n                    id: id\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                    this.rating_result = res.data;\r\n                    this.view_update_rating(index)\r\n                  })\r\n            }\r\n        },\r\n\r\n        handleScrolledToBottom(isVisible){\r\n            if (!isVisible)  { return }\r\n            if (this.all_data == null) { return }\r\n            if (this.last_datetime == null) { return }\r\n            this.get_publ()\r\n        },\r\n\r\n        go_page(){\r\n                var url_page = '/' + this.username_url;\r\n                this.$router.push(url_page).catch(()=>{});;\r\n\r\n        },\r\n\r\n        status(id){\r\n            var url_page = '/' + this.username_url + '/status/' + id;\r\n            this.$router.push(url_page);\r\n        },\r\n\r\n        get_publ(){\r\n            if (this.stop_get_pudl != 0){\r\n                axios.post(this.$hostname + '/api/mainpage/publ/get_publ/', {\r\n                    username: this.username_url,\r\n                    datetime_request: this.last_datetime\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      if (res.data != false){\r\n                          this.stop_get_pudl = res.data.length\r\n                          this.last_datetime = res.data[res.data.length-1].datetime\r\n                          for (var i = 0; i < res.data.length; i++){\r\n                                res.data[i].datetime = moment(res.data[i].datetime).format('DD MMMM YY')\r\n                          }\r\n                          this.all_data.push(...res.data);\r\n                      }\r\n\r\n                  })\r\n            }\r\n        },\r\n\r\n        SavePubl(message,images){\r\n            if (message != '' || images.length != 0){\r\n                this.stop_get_pudl = 10\r\n                this.last_datetime = null\r\n                this.all_data = []\r\n                axios.post(this.$hostname + '/api/mainpage/publ/save_publ/', {\r\n                    message: message,\r\n                    images: images,\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.get_publ()\r\n\r\n                  })\r\n\r\n            }\r\n        },\r\n\r\n\r\n        user_check(){\r\n          axios.post(this.$hostname + '/api/mainpage/user_check/get_username/', {\r\n\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.username = res.data.username;\r\n              if (this.username == this.username_url){\r\n                  this.access = true;\r\n              }\r\n          })\r\n        },\r\n        back_home(){\r\n                var url_page = '/' + this.username_url;\r\n                this.$router.push(url_page).catch(()=>{});\r\n            },\r\n\r\n    },\r\n    created() {\r\n        this.user_check()\r\n        this.get_publ()\r\n\r\n    },\r\n\r\n    filters: {\r\n        number: function(num) {\r\n            if (String(num).length < 5) {\r\n                return Math.floor(num);\r\n            }\r\n            if (String(num).length < 7 && String(num).length > 4) {\r\n                return Math.floor(num/1000) + 'т';\r\n            }\r\n            if (String(num).length < 9 && String(num).length > 6) {\r\n                return Math.floor(num/1000000) + 'м';\r\n            }\r\n        }\r\n    }\r\n\r\n\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n\r\n\r\n\r\n</style>"],"sourceRoot":"src/components/userpage"}]}