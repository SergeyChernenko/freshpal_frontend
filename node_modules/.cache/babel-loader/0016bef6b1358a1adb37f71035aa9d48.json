AEA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <br>\r\n\r\n    <div>\r\n        <p>\r\n          <input id=\"firstname\" size=\"sm\" class=\"mr-sm-2\" v-model=\"username\" placeholder=\"Имя\" name=\"firstname\"></input>\r\n        </p>\r\n        <p>\r\n          <input id=\"lastname\" size=\"sm\" class=\"mr-sm-2\" v-model=\"username\" placeholder=\"Фамилия\" name=\"lastname\"></input>\r\n        </p>\r\n        <p>\r\n          <input id=\"username\" size=\"sm\" class=\"mr-sm-2\" v-model=\"username\" placeholder=\"Имя пользователя\" name=\"username\"></input>\r\n        </p>\r\n        <p>\r\n          <input id=\"password\" size=\"sm\" class=\"mr-sm-2\" placeholder=\"Пароль\" type=\"password\" v-model=\"password\" name=\"password\"></input>\r\n        </p>\r\n    </div>\r\n\r\n    <div>\r\n        <p>\r\n            <button size=\"sm\" class=\"my-2 my-sm-0\" type=\"submit\" v-on:click=\"login(true)\">Регистрация</button>\r\n        </p>\r\n     </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport axios from 'axios'\r\n\r\n//import { TokenService } from './storage/TokenService.js'\r\n\r\nexport default {\r\n  name: 'register',\r\n  components: {\r\n\r\n  },\r\n  data() {\r\n      return{\r\n          username: '',\r\n          password: '',\r\n          token: localStorage.getItem('user-token') || null,\r\n          authorization_check: true,\r\n      }\r\n  },\r\n   methods:{\r\n      authorization_switch(flag){\r\n          this.authorization_check = flag;\r\n      },\r\n      login(flag){\r\n          this.authorization_check = flag;\r\n          axios.post('http://127.0.0.1:8000/auth/', {\r\n            username: this.username,\r\n            password: this.password,\r\n          })\r\n          .then(response => {\r\n              this.token = response.data.token;\r\n              console.log(this.token);\r\n              localStorage.setItem('user-token',response.data.token);\r\n          })\r\n          .catch(err => {\r\n              localStorage.removeItem('user-token');\r\n          })\r\n      },\r\n      logout(){\r\n          localStorage.removeItem('user-token');\r\n          this.token = null;\r\n      }\r\n   },\r\n   created() {\r\n      this.token = localStorage.getItem('user-token');\r\n\r\n   }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":"src/components/startpage"}]}