" v-for=\"mess in all_data\" v-if=\"mess.parent_id==Null\">\r\n        <div style=\"margin-bottom: 20px\" class=\"border_st\">\r\n            <div class=\"space-edit\">\r\n                <div style=\"display: flex\">\r\n                    <a class=\"sub_link\" v-on:click=\"go_page()\"><img class=\"img_p\" :src=\"url+'/media/profile/'+username_url+'/'+username_url+'.png'\"></a>\r\n                    <div class=\"username_p\">\r\n                        <a class=\"sub_link\" v-on:click=\"go_page()\">{{username_url}}</a> · {{mess.datetime}}\r\n                    </div>\r\n                </div>\r\n                <div class=\"username_p\" v-if=\"mess.edit==true\">\r\n                    <span class=\"edit_style\">изменено</span>\r\n                </div>\r\n            </div>\r\n            <div style=\"margin-top: 10px\" class=\"public_status\" v-on:click=\"status(mess.id)\">\r\n                <div class=\"just-line-break\">\r\n                    {{mess.message}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n    import Vue from 'vue'\r\n    import TextareaAutosize from 'vue-textarea-autosize'\r\n    import axios from \"axios\";\r\n\r\n\r\n    Vue.use(TextareaAutosize)\r\n\r\n    export default {\r\n        name: \"Publications\",\r\n\r\n    components: {\r\n\r\n\r\n    },\r\n    data(){\r\n      return{\r\n        url: this.$hostname,\r\n        access_token: localStorage.getItem('access') || null,\r\n        message: null,\r\n        username_url: this.$route.params.username,\r\n        access: false,\r\n        all_data: null,\r\n        last_datetime: null,\r\n        stop_get_comment: 8,\r\n\r\n      }\r\n    },\r\n    props: {\r\n          username: String,\r\n    },\r\n    methods: {\r\n\r\n        go_page(){\r\n                var url_page = '/' + this.username_url;\r\n                this.$router.push(url_page);\r\n\r\n        },\r\n\r\n        status(id){\r\n            var url_page = '/' + this.username_url + '/status/' + id;\r\n            this.$router.push(url_page);\r\n        },\r\n\r\n        get_mess(){\r\n            axios.post(this.url + '/api/mainpage/wall/get_mess/', {\r\n                username: this.username_url,\r\n              },\r\n          {\r\n                headers: {\r\n                    'Authorization': 'Bearer ' + this.access_token,\r\n                }\r\n            })\r\n              .then(res => {\r\n                this.all_data = res.data;\r\n\r\n              })\r\n        },\r\n\r\n        save_mess(){\r\n            if (this.message != null && this.message != ''){\r\n                axios.post(this.url + '/api/mainpage/wall/save_mess/', {\r\n                    message: this.message\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.get_mess()\r\n\r\n                  })\r\n                this.message = null;\r\n            }\r\n        },\r\n\r\n        user_check(){\r\n          if (this.username == this.username_url) {\r\n              this.access = true;\r\n          }\r\n        },\r\n        back_home(){\r\n                var url_page = '/' + this.username_url;\r\n                this.$router.push(url_page);\r\n            },\r\n\r\n    },\r\n    created() {\r\n        setTimeout(() => this.user_check(), 200);\r\n        this.get_mess()\r\n\r\n    },\r\n\r\n\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n\r\n\r\n\r\n</style>"]}]}