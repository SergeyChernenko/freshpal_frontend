data\">\r\n        <div class=\"border_st public_status\" v-on:click=\"status(mess[1][0])\">\r\n            <div style=\"display: flex\">\r\n                 <img class=\"img_p\" :src=\"url+'/media/profile/'+username_url+'/'+username_url+'.png'\">\r\n                <div class=\"username_p\">\r\n                    {{username_url}} · {{mess[0]}}\r\n                </div>\r\n\r\n            </div>\r\n            <div style=\"height: 8px\"></div>\r\n            <div class=\"just-line-break\">\r\n                {{mess[1][1]}}\r\n            </div>\r\n            <div style=\"height: 20px\"></div>\r\n\r\n        </div>\r\n        <div style=\"height: 20px\"></div>\r\n    </div>\r\n\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\n    import axios from \"axios\";\r\n\r\n    export default {\r\n        name: \"Publications\",\r\n\r\n    components: {\r\n\r\n\r\n    },\r\n    data(){\r\n      return{\r\n        url: this.$hostname,\r\n        access_token: localStorage.getItem('access') || null,\r\n        message: null,\r\n        username_url: this.$route.params.username,\r\n        access: false,\r\n        all_data: null,\r\n      }\r\n    },\r\n    props: {\r\n          username: String,\r\n    },\r\n    methods: {\r\n\r\n        status(id){\r\n            var url_page = '/' + this.username + '/status/' + id;\r\n            this.$router.push(url_page);\r\n        },\r\n\r\n        resize(event) {\r\n          event.target.style.height = \"auto\";\r\n          event.target.style.height = `${event.target.scrollHeight}px`;\r\n        },\r\n\r\n        get_mess(){\r\n            axios.post(this.url + '/api/mainpage/wall/get_mess/', {\r\n                username: this.username_url,\r\n              },\r\n          {\r\n                headers: {\r\n                    'Authorization': 'Bearer ' + this.access_token,\r\n                }\r\n            })\r\n              .then(res => {\r\n                this.all_data = res.data;\r\n\r\n              })\r\n        },\r\n\r\n        save_mess(){\r\n            if (this.message != null && this.message != ''){\r\n                axios.post(this.url + '/api/mainpage/wall/save_mess/', {\r\n                    message: this.message\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.get_mess()\r\n\r\n                  })\r\n                this.message = null;\r\n            }\r\n        },\r\n\r\n        user_check(){\r\n\r\n          if (this.username == this.username_url) {\r\n              this.access = true;\r\n          }\r\n\r\n        },\r\n        back_home(){\r\n                var url_page = '/' + this.username_url;\r\n                this.$router.push(url_page);\r\n            },\r\n\r\n    },\r\n    created() {\r\n        setTimeout(() => this.user_check(), 200);\r\n        this.get_mess()\r\n\r\n    },\r\n\r\n\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n\r\n\r\n\r\n</style>"]}]}