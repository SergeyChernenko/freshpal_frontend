class=\"border_st\">\r\n            <div class=\"space-edit\">\r\n                <div style=\"display: flex\">\r\n                    <a class=\"sub_link\" v-on:click=\"go_page()\"><img v-if=\"username_url_id != null\" class=\"img_p\" :src=\"url+'/media/profile/'+username_url_id+'/'+username_url_id+'.png'\"></a>\r\n                    <div class=\"username_p\">\r\n                        <a class=\"sub_link\" v-on:click=\"go_page()\">{{username_url}}</a> · {{mess.datetime}}\r\n                    </div>\r\n                </div>\r\n                <div class=\"username_p\" v-if=\"mess.edit==true\">\r\n                    <span class=\"edit_style\">изменено</span>\r\n                </div>\r\n            </div>\r\n            <div class=\"div_img_show_publ\" v-if=\"mess.images.length!=0\">\r\n                {{mess.images}}\r\n<!--                        <img style=\"max-height: 300px\" :src=\"mess.images\">-->\r\n            </div>\r\n            <div style=\"margin-top: 10px\" class=\"public_status\" v-on:click=\"status(mess.id)\">\r\n                <div class=\"just-line-break\">\r\n                    {{mess.message}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div v-observe-visibility=\"handleScrolledToBottom\"></div>\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n    import Vue from 'vue'\r\n    import TextareaAutosize from 'vue-textarea-autosize'\r\n    import axios from \"axios\";\r\n    import moment from \"moment\";\r\n    import VueObserveVisibility from \"vue-observe-visibility\";\r\n    import Textarea from \"../Other/Textarea\"\r\n\r\n\r\n    Vue.use(TextareaAutosize)\r\n\r\n    export default {\r\n        name: \"Publications\",\r\n\r\n    components: {\r\n        Textarea\r\n    },\r\n\r\n\r\n\r\n    data(){\r\n      return{\r\n        url: this.$hostname,\r\n        access_token: localStorage.getItem('access') || null,\r\n        username_url: this.$route.params.username,\r\n        access: false,\r\n        all_data: [],\r\n        last_datetime: null,\r\n        stop_get_pudl: 8,\r\n        username: null,\r\n        description: 'Что у вас нового?'\r\n      }\r\n    },\r\n    props: {\r\n          username_url_id: Number,\r\n    },\r\n    methods: {\r\n\r\n\r\n\r\n        handleScrolledToBottom(isVisible){\r\n            if (!isVisible)  { return }\r\n            if (this.all_data == null) { return }\r\n            if (this.last_datetime == null) { return }\r\n            this.get_publ()\r\n        },\r\n\r\n        go_page(){\r\n                var url_page = '/' + this.username_url;\r\n                this.$router.push(url_page);\r\n\r\n        },\r\n\r\n        status(id){\r\n            var url_page = '/' + this.username_url + '/status/' + id;\r\n            this.$router.push(url_page);\r\n        },\r\n\r\n        get_publ(){\r\n            if (this.stop_get_pudl != 0){\r\n                axios.post(this.url + '/api/mainpage/publ/get_publ/', {\r\n                    username: this.username_url,\r\n                    datetime_request: this.last_datetime\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      if (res.data != false){\r\n                          this.stop_get_pudl = res.data.length\r\n                          this.last_datetime = res.data[res.data.length-1].datetime\r\n                          for (var i = 0; i < res.data.length; i++){\r\n                                res.data[i].datetime = moment(res.data[i].datetime).format('DD MMMM YY')\r\n                          }\r\n                          this.all_data.push(...res.data);\r\n                      }\r\n\r\n                  })\r\n            }\r\n        },\r\n\r\n        SaveMessage(message,images){\r\n            if (message != null || images.length != 0){\r\n                this.stop_get_pudl = 10\r\n                this.last_datetime = null\r\n                this.all_data = []\r\n                axios.post(this.url + '/api/mainpage/publ/save_publ/', {\r\n                    message: message,\r\n                    images: images,\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.get_publ()\r\n\r\n                  })\r\n\r\n            }\r\n        },\r\n\r\n\r\n        user_check(){\r\n          axios.post(this.url + '/api/mainpage/user_check/get_username/', {\r\n\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.username = res.data.username;\r\n              if (this.username == this.username_url){\r\n                  this.access = true;\r\n              }\r\n          })\r\n        },\r\n        back_home(){\r\n                var url_page = '/' + this.username_url;\r\n                this.$router.push(url_page);\r\n            },\r\n\r\n    },\r\n    created() {\r\n        this.user_check()\r\n        this.get_publ()\r\n\r\n    },\r\n\r\n\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n\r\n\r\n\r\n</style>"]}]}