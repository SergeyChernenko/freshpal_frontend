          </div>\r\n                <div class=\"itm_1_2\"></div>\r\n                <div class=\"itm_1_3\">\r\n                    <p><a class=\"a_style\" v-on:click=\"\">Редактировать страницу</a></p>\r\n                    <button v-if=\"access != true && access_token != null && check_sub_res == false\" class=\"button_style\" v-on:click=\"subscribe()\">Подписаться</button>\r\n                    <button v-if=\"access != true && access_token != null && check_sub_res == true\" class=\"button_style\" v-on:click=\"unsubscribe()\">Отписаться</button>\r\n                </div>\r\n            </div>\r\n\r\n            <p>\r\n\r\n            </p>\r\n        </div>\r\n    </div>\r\n    <div class=\"mainitem\" v-if=\"username_url_check == false\">\r\n        <div class=\"center_text\">\r\n            <div style=\"height: 100px\"></div>\r\n            <p style=\"text-align:  center\" align=\"left\"><font size=\"4\" color=\"#fcfcfc\" face=\"Trebuchet MS\">Пользователя {{ this.username_url }} не существует.</font></p>\r\n            <div v-if=\"access_token == null\">\r\n                <p style=\"text-align:  center\" align=\"left\"><a class=\"a_style\" v-on:click=\"create_new_page()\">Создать страницу</a></p>\r\n            </div>\r\n        </div>\r\n    </div>\r\n   <div class=\"mainitem\" v-if=\"username_url_check == null\">\r\n    </div>\r\n\r\n    <modal name=\"followers\"\r\n       :width=\"420\"\r\n       :height=\"480\"\r\n       :pivot-y=\"0.5\"\r\n       :adaptive=\"false\"\r\n       :scrollable=\"true\"\r\n       styles=\"border-radius: 17px; background: #444444; box-shadow: 0 0 1px 1px #5D5D5D\"\r\n       transition=\"nice-modal-fade\">\r\n        <font size=\"5\" color=\"#fcfcfc\" face=\"Trebuchet MS\">Подписчики</font>\r\n        <hr>\r\n        <div v-for=\"follower in list_followers\">\r\n            <div class=\"list_modal_style\">\r\n                <p><a class=\"a_style\" :href=\"'#/'+follower\"><font size=\"4\" color=\"#fcfcfc\" face=\"Trebuchet MS\">{{follower}}</font></a></p>\r\n            </div>\r\n        </div>\r\n    </modal>\r\n\r\n    <modal name=\"following\"\r\n       :width=\"420\"\r\n       :height=\"480\"\r\n       :pivot-y=\"0.5\"\r\n       :adaptive=\"true\"\r\n       :scrollable=\"true\"\r\n       styles=\"border-radius: 17px; background: #444444; box-shadow: 0 0 1px 1px #5D5D5D\"\r\n       transition=\"nice-modal-fade\">\r\n       <font size=\"5\" color=\"#fcfcfc\" face=\"Trebuchet MS\">Подписки</font>\r\n        <hr>\r\n        <div v-for=\"following in list_following\">\r\n            <div class=\"list_modal_style\">\r\n                <p><a class=\"a_style\" :href=\"'#/'+following\"><font size=\"4\" color=\"#fcfcfc\" face=\"Trebuchet MS\">{{following}}</font></a></p>\r\n            </div>\r\n        </div>\r\n    </modal>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\nimport axios from 'axios';\r\nimport VModal from 'vue-js-modal';\r\nimport  Vue from  'vue';\r\n\r\nVue.use(VModal);\r\n\r\nexport default {\r\n  name: 'startpage',\r\n  components: {\r\n\r\n  },\r\n\r\n  data(){\r\n      return{\r\n        url: 'http://127.0.0.1:8000',\r\n        access_token: localStorage.getItem('access') || null,\r\n        refresh_token: localStorage.getItem('refresh') || null,\r\n        username_url: this.$route.params.username,\r\n        username: null,\r\n        access: null,\r\n        username_url_check: null,\r\n        sum_sub: Array,\r\n        check_sub_res: null,\r\n        selected_avatar: null,\r\n        url_avatar: null,\r\n        list_followers: Array,\r\n        list_following: Array,\r\n      }\r\n  },\r\n\r\n  methods: {\r\n\r\n    send_url(name){\r\n        var url_page = '/' + name;\r\n        this.$router.push(url_page);\r\n    },\r\n\r\n    // avatar_selected(event){\r\n    //     this.selectedAvatar = event.target.files[0];\r\n    //     this.url_avatar = URL.createObjectURL(this.selectedAvatar);\r\n    //     console.log(this.url_avatar)\r\n    // },\r\n    //\r\n    // upload_avatar() {\r\n    //\r\n    // },\r\n\r\n    followers_show(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/list_followers/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.list_followers = res.data;\r\n          })\r\n        this.$modal.show('followers');\r\n    },\r\n    // followers_hide () {\r\n    //     this.$modal.hide('followers');\r\n    // },\r\n    following_show(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/list_following/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n            headers: {\r\n                'Authorization': 'Bearer ' + this.access_token,\r\n            }\r\n        })\r\n          .then(res => {\r\n              this.list_following = res.data;\r\n              console.log(this.list_following)\r\n          })\r\n        this.$modal.show('following');\r\n    },\r\n\r\n    check_sub(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/check_sub/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data;\r\n          })\r\n    },\r\n\r\n    get_sum_sub(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/sum/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.sum_sub = res.data;\r\n          })\r\n    },\r\n\r\n    subscribe(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/plus/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data\r\n          })\r\n    },\r\n\r\n    unsubscribe(){\r\n        axios.post(this.url + '/api/mainpage/subscribe/minus/', {\r\n            username: this.username_url,\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.check_sub_res = res.data;\r\n          })\r\n    },\r\n\r\n    back_home(){\r\n        var currentLocation = window.location + '';\r\n        let arr = new Array();\r\n        arr = currentLocation.split('/')\r\n        if (arr[4] != this.username)\r\n        {\r\n            var url_page = '/' + this.username;\r\n            this.$router.push(url_page);\r\n        }\r\n    },\r\n\r\n    create_new_page(){\r\n        this.$router.push('/')\r\n    },\r\n\r\n    user_url_check(){\r\n\r\n        axios.post(this.url + '/api/mainpage/user_url_check/check/', {\r\n            username: this.username_url,\r\n          })\r\n          .then(res => {\r\n              this.username_url_check = res.data;\r\n\r\n          })\r\n\r\n    },\r\n\r\n    fun_after_token(){\r\n         this.check_sub();\r\n    },\r\n\r\n    user_check(){\r\n      axios.post(this.url + '/api/mainpage/user_check/get_username/', {\r\n\r\n          },\r\n      {\r\n\t\t\theaders: {\r\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\r\n\t\t\t}\r\n\t\t})\r\n          .then(res => {\r\n              this.username = res.data;\r\n              this.fun_after_token();\r\n              if (this.username == this.username_url){\r\n                  this.access = true;\r\n              }\r\n          })\r\n\r\n    },\r\n\r\n    logout(){\r\n      localStorage.removeItem('access');\r\n      localStorage.removeItem('refresh');\r\n      this.access_token = null;\r\n      this.refresh_token = null;\r\n      this.$router.push('/')\r\n    },\r\n\r\n  },\r\n  created() {\r\n    this.user_url_check(); //существует ли пользователь\r\n    this.get_sum_sub(); // кол-во подписчиков и подписок у пользователя\r\n    this.access_token = localStorage.getItem('access');\r\n    this.refresh_token = localStorage.getItem('refresh');\r\n    if (this.access_token != null){\r\n      this.user_check()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .header {\r\n\theight: 54px;\r\n\tbackground: #333333;\r\n    border: 1px solid #444444;\r\n\t}\r\n  .flex {\r\n    display: flex;\r\n    /*justify-content: center;*/\r\n\t/*background: #9ba5b3; padding: 5px;*/\r\n  }\r\n  .itm_logo {\r\n    color: #eee;\r\n    margin: 6px;\r\n    /*flex-grow:1;*/\r\n    /*padding: 10px;*/\r\n    /*font-size: 2.5rem;*/\r\n  }\r\n  .itm_search{\r\n      margin: 10px;\r\n  }\r\n  .it_indent{\r\n      flex-basis: 10%\r\n  }\r\n  .itm_2{\r\n      flex-basis: 100%\r\n  }\r\n  .search{\r\n    width: 250px;\r\n    height: 30px;\r\n  }\r\n  .mainitem{\r\n      background: #252525;\r\n      min-height: 100vh;\r\n  }\r\n  .sideitem_1{\r\n      display: flex;\r\n      height: 200px;\r\n      flex-wrap: wrap;\r\n      align-content: center;\r\n  }\r\n  .itm_1_1{\r\n      flex-basis: 15%\r\n  }\r\n  .itm_1_2{\r\n      flex-basis: 60px;\r\n  }\r\n  .itm_1_3{\r\n\r\n      /*align-self: center;*/\r\n  }\r\n\r\n  .center_text{\r\n      margin-left: auto;\r\n      margin-right: auto;\r\n      width: 350px;\r\n  }\r\n\r\n  .list_modal_style{\r\n      width: 100%;\r\n      text-align: left;\r\n      padding: 5px 0 0 15px;\r\n  }\r\n\r\n  hr{\r\n      border: none; /* Убираем границу */\r\n      background-color: #fcfcfc; /* Цвет линии */\r\n      color: #fcfcfc; /* Цвет линии для IE6-7 */\r\n      height: 1px; /* Толщина линии */\r\n  }\r\n\r\n</style>\r\n"]}]}