ок.</font></p>\n\t\t\t\t\t\t<p align=\"left\"><font size=\"4\" color=\"#262626\" face=\"Arial\">Присоединяйся к нам прямо сейчас!</font></p>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t\t<div class=\"sideitem\">\n\t\t\t\t<div class=\"login\">\n\t\t\t\t\t<Authorization/>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"register\">\n\t\t\t\t\t<Registration/>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t\t<div class=\"sideitem_2\">\n\t\t\t\t<div class=\"item_1\"></div>\n\t\t\t\t<div class=\"item_2\"></div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n\n<!--    <footer class=\"footer\">-->\n<!--        FreshPal 2021-->\n<!--    </footer>&lt;!&ndash; .footer &ndash;&gt;-->\n\n  </div>\n  </div>\n\n</template>\n\n<script>\n// @ is an alias to /src\nimport Authorization from \"../components/startpage/Authorization\";\nimport Registration from \"../components/startpage/Registration\"\nimport axios from \"axios\";\n\nexport default {\n  name: 'Home',\n  components: {\n    Authorization,\n    Registration\n  },\n\n\tdata() {\n      return{\n      \turl: 'http://127.0.0.1:8000',\n      \taccess_token: localStorage.getItem('access') || null,\n\t  \trefresh_token: localStorage.getItem('refresh') || null,\n\t  }\n  \t},\n\n\tmethods:{\n  \t\tget_new_tokens(){\n        axios.post(this.url + '/api/token/refresh/', {\n            refresh: this.refresh_token,\n          })\n          .then(response => {\n              this.access_token = response.data.access;\n              this.refresh_token = response.data.refresh;\n              localStorage.setItem('access',response.data.access);\n              localStorage.setItem('refresh',response.data.refresh);\n              this.get_user_data()\n          })\n          .catch(err => {\n              localStorage.removeItem('access');\n              localStorage.removeItem('refresh');\n              this.access_token = null;\n              this.refresh_token = null;\n          })\n      },\n\n\t\tget_user_data(){\n\n\t\taxios.post(this.url + '/api/mainpage/userdata/get_user_data/', {\n\n\t\t},{\n\t\t\theaders: {\n\t\t\t\t'Authorization': 'Bearer ' + this.access_token,\n\t\t\t}\n\t\t})\n\t\t  .then(response => {\n\t\t\t  this.get_name = response.data;\n\t\t\t  var url_page = '/'+ this.get_name;\n\t\t\t  this.$router.push(url_page)\n\t\t  })\n\t\t  .catch(err => {\n\t\t\t  console.log(err);\n\t\t\t  this.get_new_tokens()\n\t\t  })\n\t\t},\n\t\t},\n\n\tcreated(){\n  \t\tthis.access_token = localStorage.getItem('access');\n      \tthis.refresh_token = localStorage.getItem('refresh');\n\t\tif (this.access_token != null){\n\t\t\tthis.get_user_data()\n\t\t}\n\n\t},\n}\n</script>\n\n<style scoped>\n.header {\n\theight: 80px;\n\t/*background: #FFE680;*/\n\t}\n\t.footer {\n\t\tmargin: -50px auto 0;\n\t\tmin-width: 1000px;\n\t\theight: 50px;\n\t\t/*background: #BFF08E;*/\n\t\tposition: relative;\n\t}\n\t.color_grey{\n\t\tbackground: #fafafa;\n\t}\n\t.flex{\n\t\t\tdisplay: flex;\n\t\t\t/*margin: 0 auto; !* выравниваем блок по центру *!*/\n\n    }\n\t.logo{\n\t\tdisplay: flex;\n  \t\tflex-flow: column;\n\t}\n\t.logo_text{\n\t\twidth: 400px;\n\t}\n    .mainitem{\n\t\tflex-grow:1; /* растягивание элементов */\n        /*background:#b5ced8;*/\n        display: flex;\n        justify-content: center;\n        min-height:300px; /* высота основного блока */\n\n    }\n\n    .sideitem{\n        /* flex:0 0 150px; */\n        flex-basis:340px; /* ширина 150 */\n        /*flex-shrink:0; !* Убираем уменьшение = 150 *!*/\n\n        display: flex;\n        flex-direction: column;\n    }\n    .sideitem_2{\n        /* flex:0 0 150px; */\n        flex-basis:250px; /* ширина 150 */\n        /*flex-shrink:0; !* Убираем уменьшение = 150 *!*/\n\n        display: flex;\n        flex-direction: column;\n    }\n\n    .login{\n        flex-basis:50%;\n        min-height:280px;\n        margin-left:10px;\n\t\tflex-grow:1; /* растягивание элементов */\n        background: #ffffff;\n        display: flex;\n        justify-content: center;\n\t\tborder-radius: 10px;\n\t\tborder: 1px solid #dbdbdb; /* Параметры границы */\n    }\n    .login:first-child{ margin-bottom:10px; }\n\t.register{\n        flex-basis:50%;\n        min-height:240px;\n        margin-left:10px;\n\t\tflex-grow:1; /* растягивание элементов */\n        background: #ffffff;\n        display: flex;\n        justify-content: center;\n\t\tborder-radius: 10px;\n\t\tborder: 1px solid #dbdbdb; /* Параметры границы */\n    }\n    .register:first-child{ margin-bottom:10px; }\n\n\t.item_1{\n        flex-basis:50%;\n        min-height:280px;\n        margin-left:10px;\n\t\tflex-grow:1; /* растягивание элементов */\n        /*background:#b5ced8;*/\n        display: flex;\n        justify-content: center;\n    }\n    .item_1:first-child{ margin-bottom:10px; }\n\n\t.item_2{\n        flex-basis:50%;\n        min-height:240px;\n        margin-left:10px;\n\t\tflex-grow:1; /* растягивание элементов */\n        /*background:#b5ced8;*/\n        display: flex;\n        justify-content: center;\n    }\n    .item_2:first-child{ margin-bottom:10px; }\n\n    /* меньше 600px */\n    @media screen and (max-width: 600px) {\n        .flex{ flex-wrap: wrap; } /* для переноса */\n\n        .sideitem{ flex-direction:row; flex-basis:100%; }\n\n        .login{ margin-left:0; margin-top:10px; }\n        .login:first-child{ margin-bottom:0; margin-right:10px; }\n\t\t.register{ margin-left:0; margin-top:10px; }\n        .register:first-child{ margin-bottom:0; margin-right:10px; }\n\n\t\t.item_1{ margin-left:0; margin-top:10px; }\n        .item_1:first-child{ margin-bottom:0; margin-right:10px; }\n\t\t.item_2{ margin-left:0; margin-top:10px; }\n        .item_2:first-child{ margin-bottom:0; margin-right:10px; }\n    }\n\n    /* меньше 450px */\n    @media screen and (max-width: 450px) {\n        .sideitem{ flex-wrap: wrap; } /* разрешаем перенос */\n        .login{ flex-basis:100%; } /* на всю ширину */\n        .login:first-child{ margin-right:0; }\n\t\t.register{ flex-basis:100%; } /* на вс