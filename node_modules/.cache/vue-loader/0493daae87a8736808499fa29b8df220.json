           .then(res => {\r\n                      this.username = res.data;\r\n                      if (this.username == this.data_mess.username){\r\n                          this.access = true;\r\n                      }\r\n                  })\r\n\r\n            },\r\n\r\n        },\r\n        created() {\r\n            this.check_id_and_username()\r\n            setTimeout(() => this.user_check(), 200);\r\n\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n.parent {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  align-content: center;\r\n  height: 100%;\r\n  justify-content: flex-end;\r\n}\r\n.child-active {\r\n  flex-basis: 30%;\r\n}\r\n\r\n</style>"]}]}