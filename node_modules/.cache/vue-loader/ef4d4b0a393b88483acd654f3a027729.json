height: 16px\"></div>\r\n    <div v-for=\"mess in all_data\">\r\n        <div class=\"border_st\">\r\n            <div style=\"display: flex\">\r\n                 <a class=\"sub_link\" v-on:click=\"back_home()\"><img class=\"img_p\" :src=\"url+'/media/profile/'+username_url+'/'+username_url+'.png'\"></a>\r\n                <div class=\"username_p\">\r\n                    <a class=\"sub_link\" v-on:click=\"back_home()\">{{username_url}}</a>  · {{mess[0]}}\r\n                </div>\r\n\r\n            </div>\r\n            <div style=\"height: 8px\"></div>\r\n            <div id=\"just-line-break\">\r\n                {{mess[1]}}\r\n            </div>\r\n        </div>\r\n        <div style=\"height: 20px\"></div>\r\n    </div>\r\n\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\n    import axios from \"axios\";\r\n\r\n    export default {\r\n        name: \"Publications\",\r\n\r\n    components: {\r\n\r\n\r\n    },\r\n    data(){\r\n      return{\r\n        url: this.$hostname,\r\n        access_token: localStorage.getItem('access') || null,\r\n        message: null,\r\n        username_url: this.$route.params.username,\r\n        access: false,\r\n        all_data: null,\r\n      }\r\n    },\r\n    props: {\r\n          username: String,\r\n    },\r\n    methods: {\r\n\r\n        resize(event) {\r\n          event.target.style.height = \"auto\";\r\n          event.target.style.height = `${event.target.scrollHeight}px`;\r\n        },\r\n\r\n        get_mess(){\r\n            axios.post(this.url + '/api/mainpage/wall/get_mess/', {\r\n                username: this.username_url,\r\n              },\r\n          {\r\n                headers: {\r\n                    'Authorization': 'Bearer ' + this.access_token,\r\n                }\r\n            })\r\n              .then(res => {\r\n                this.all_data = res.data;\r\n\r\n              })\r\n        },\r\n\r\n        save_mess(){\r\n            if (this.message != null && this.message != ''){\r\n                axios.post(this.url + '/api/mainpage/wall/save_mess/', {\r\n                    message: this.message\r\n                  },\r\n              {\r\n                    headers: {\r\n                        'Authorization': 'Bearer ' + this.access_token,\r\n                    }\r\n                })\r\n                  .then(res => {\r\n                      this.get_mess()\r\n\r\n                  })\r\n                this.message = null;\r\n            }\r\n        },\r\n\r\n        user_check(){\r\n\r\n          if (this.username == this.username_url) {\r\n              this.access = true;\r\n          }\r\n\r\n        },\r\n        back_home(){\r\n                var currentLocation = window.location + '';\r\n                let arr = new Array();\r\n                arr = currentLocation.split('/')\r\n                if (arr[4] != this.username_url)\r\n                {\r\n                    var url_page = '/' + this.username_url;\r\n                    this.$router.push(url_page);\r\n                }\r\n            },\r\n\r\n    },\r\n    created() {\r\n        setTimeout(() => this.user_check(0), 200);\r\n        this.get_mess()\r\n\r\n    },\r\n\r\n\r\n    }\r\n\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n    \r\n    .img_p{\r\n      border-radius: 50%;\r\n      width: 45px;\r\n      height: 45px;\r\n  }\r\n    .username_p{\r\n        margin: 10px 10px 10px 15px;\r\n    }\r\n    .img_input_m{\r\n        margin: 10px 10px 10px 15px;\r\n        border-radius: 50%;\r\n        width: 45px;\r\n        height: 45px;\r\n    }\r\n    .but_input_m{\r\n        height: 45px;\r\n        width: 200px;\r\n        margin: 10px 10px 10px 10px;\r\n    }\r\n    #just-line-break {\r\n      white-space: pre-line;\r\n    }\r\n\r\n\r\n\r\n\r\n</style>"]}]}