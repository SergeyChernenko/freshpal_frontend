div>\r\n        </div>\r\n<!--        <div style=\"height: 16px\"></div>-->\r\n<!--        <div class=\"donate_style\">-->\r\n<!--            <p class=\"tags\">Подписчики</p>-->\r\n<!--        </div>-->\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n    name: \"RighMenu\",\r\n    components: {\r\n\r\n    },\r\n    data() {\r\n        return {\r\n            access_token: localStorage.getItem('access') || null,\r\n            new_mess: null,\r\n            hashtag: [],\r\n        }\r\n    },\r\n    props: {\r\n      username: String,\r\n    },\r\n    methods: {\r\n\r\n        hashtag_page(tag){\r\n            var url_page = '/hashtag/' + tag;\r\n            this.$router.push(url_page).catch(()=>{});\r\n        },\r\n\r\n        hashtag_main(){\r\n            var url_page = '/hashtag';\r\n            this.$router.push(url_page).catch(()=>{});\r\n        },\r\n\r\n        get_hashtag(){\r\n            axios.post(this.$hostname + '/api/home/home/get_hashtag_right/', {\r\n\r\n              },\r\n          {\r\n                headers: {\r\n                    'Authorization': 'Bearer ' + this.access_token,\r\n                }\r\n            })\r\n              .then(res => {\r\n                  this.hashtag = res.data;\r\n              })\r\n        },\r\n\r\n    },\r\n    created() {\r\n        this.get_hashtag()\r\n    },\r\n    filters: {\r\n        number: function(num) {\r\n            if (String(num).length < 5) {\r\n                return Math.floor(num);\r\n            }\r\n            if (String(num).length < 7 && String(num).length > 4) {\r\n                return Math.floor(num/1000) + 'т';\r\n            }\r\n            if (String(num).length < 9 && String(num).length > 6) {\r\n                return Math.floor(num/1000000) + 'м';\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}